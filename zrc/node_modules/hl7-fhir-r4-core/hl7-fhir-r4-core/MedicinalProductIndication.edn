{ns hl7-fhir-r4-core.MedicinalProductIndication,
 import
 #{hl7-fhir-r4-core.Substance
   hl7-fhir-r4-core.BackboneElement
   zen.fhir
   hl7-fhir-r4-core.MedicinalProductUndesirableEffect
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.SubstanceSpecification
   hl7-fhir-r4-core.Population
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.MedicinalProduct
   hl7-fhir-r4-core.Medication},
 schema
 {:zen/tags #{zen/schema zen.fhir/base-schema},
  :zen/desc "Indication for the Medicinal Product.",
  :zen.fhir/type "MedicinalProductIndication",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/MedicinalProductIndication",
  :zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :type zen/map,
  :keys
  {:diseaseSymptomProcedure
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The disease, symptom or procedure that is the indication for treatment"},
   :undesirableEffect
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.MedicinalProductUndesirableEffect/schema}},
     :zen/desc
     "Describe the undesirable effects of the medicinal product"}},
   :duration
   {:confirms #{hl7-fhir-r4-core.Quantity/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "Timing or duration information as part of the indication"},
   :otherTherapy
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:therapyRelationshipType
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The type of relationship between the medicinal product indication or contraindication and another therapy"},
      :medication
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:CodeableConcept :Reference}},
       :type zen/map,
       :keys
       {:CodeableConcept
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
        :Reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference
         {:refers
          #{hl7-fhir-r4-core.Medication/schema
            hl7-fhir-r4-core.SubstanceSpecification/schema
            hl7-fhir-r4-core.Substance/schema
            hl7-fhir-r4-core.MedicinalProduct/schema}}}},
       :fhir/flags #{:SU},
       :zen/desc
       "Reference to a specific medication (active substance, medicinal product or class of products) as part of an indication or contraindication"}},
     :require #{:therapyRelationshipType :medication},
     :fhir/flags #{:SU},
     :zen/desc
     "Information about the use of the medicinal product in relation to other therapies described as part of the indication"}},
   :comorbidity
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Comorbidity (concurrent condition) or co-infection as part of the indication"}},
   :intendedEffect
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The intended effect, aim or strategy to be achieved by the indication"},
   :population
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Population/schema},
     :fhir/flags #{:SU},
     :zen/desc "The population group to which this applies"}},
   :diseaseStatus
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The status of the disease or symptom for which the indication applies"},
   :subject
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Medication/schema
        hl7-fhir-r4-core.MedicinalProduct/schema}},
     :zen/desc "The medication for which this is an indication"}}}}}