{ns hl7-fhir-r4-core.DeviceDefinition,
 import
 #{hl7-fhir-r4-core.value-set.device-nametype
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.ContactPoint
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.ProdCharacteristic
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.DeviceDefinition
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization
   hl7-fhir-r4-core.ProductShelfLife},
 schema
 {:zen/tags #{zen/schema zen.fhir/base-schema},
  :zen/desc
  "The characteristics, operational status and capabilities of a medical-related component of a medical device.",
  :zen.fhir/type "DeviceDefinition",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/DeviceDefinition",
  :zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :type zen/map,
  :keys
  {:deviceName
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:name
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "The name of the device"},
      :_name {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :type
      {:confirms #{hl7-fhir-r4-core.code/schema},
       :zen.fhir/value-set
       {:symbol hl7-fhir-r4-core.value-set.device-nametype/value-set,
        :strength :required},
       :zen/desc
       "udi-label-name | user-friendly-name | patient-reported-name | manufacturer-name | model-name | other"},
      :_type {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:name :type},
     :zen/desc "A name given to the device to identify it"}},
   :shelfLifeStorage
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ProductShelfLife/schema},
     :zen/desc "Shelf Life and storage information"}},
   :property
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc
       "Code that specifies the property DeviceDefinitionPropetyCode (Extensible)"},
      :valueQuantity
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.Quantity/schema},
        :zen/desc "Property value as a quantity"}},
      :valueCode
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :zen/desc
        "Property value as a code, e.g., NTP4 (synced to NTP)"}}},
     :require #{:type},
     :zen/desc
     "The actual configuration settings of a device as it actually operates, e.g., regulation status, time properties"}},
   :_onlineInformation {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_modelNumber {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :modelNumber
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "The model number for the device"},
   :udiDeviceIdentifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:deviceIdentifier
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc
       "The identifier that is to be associated with every Device that references this DeviceDefintiion for the issuer and jurisdication porvided in the DeviceDefinition.udiDeviceIdentifier"},
      :_deviceIdentifier
      {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :issuer
      {:confirms #{hl7-fhir-r4-core.uri/schema},
       :zen/desc
       "The organization that assigns the identifier algorithm"},
      :_issuer {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :jurisdiction
      {:confirms #{hl7-fhir-r4-core.uri/schema},
       :zen/desc
       "The jurisdiction to which the deviceIdentifier applies"},
      :_jurisdiction {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:deviceIdentifier :jurisdiction :issuer},
     :zen/desc "Unique Device Identifier (UDI) Barcode string"}},
   :type
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "What kind of device or device system this is"},
   :capability
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Type of capability"},
      :description
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :zen/desc "Description of capability"}}},
     :require #{:type},
     :zen/desc "Device capabilities"}},
   :specialization
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:systemType
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc
       "The standard that is used to operate and communicate"},
      :_systemType {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :version
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc
       "The version of the standard that is used to operate and communicate"},
      :_version {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:systemType},
     :zen/desc
     "The capabilities supported on a  device, the standards to which the device conforms for a particular purpose, and used for the communication"}},
   :parentDevice
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.DeviceDefinition/schema}},
    :zen/desc "The parent device it can be part of"},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Device notes and comments"}},
   :languageCode
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc
     "Language code for the human-readable text strings produced by the device (all supported)"}},
   :safety
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Safety characteristics of the device"}},
   :material
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:substance
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "The substance"},
      :alternate
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :zen/desc "Indicates an alternative material of the device"},
      :_alternate {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :allergenicIndicator
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :zen/desc
       "Whether the substance is a known or suspected allergen"},
      :_allergenicIndicator
      {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:substance},
     :zen/desc
     "A substance used to create the material(s) of which the device is made"}},
   :url
   {:confirms #{hl7-fhir-r4-core.uri/schema},
    :zen/desc "Network address to contact device"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc "Instance identifier"}},
   :manufacturer
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:string :Reference}},
    :type zen/map,
    :keys
    {:string {:confirms #{hl7-fhir-r4-core.string/schema}},
     :_string {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :Reference
     {:confirms
      #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
      :zen.fhir/reference
      {:refers #{hl7-fhir-r4-core.Organization/schema}}}},
    :zen/desc "Name of device manufacturer"},
   :quantity
   {:confirms #{hl7-fhir-r4-core.Quantity/schema},
    :zen/desc
    "The quantity of the device present in the packaging (e.g. the number of devices present in a pack, or the number of devices in the same package of the medicinal product)"},
   :version
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.string/schema},
     :zen/desc "Available versions"}},
   :_version
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :contact
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactPoint/schema},
     :zen/desc "Details for human/organization for support"}},
   :owner
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Organization responsible for device"},
   :_url {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :onlineInformation
   {:confirms #{hl7-fhir-r4-core.uri/schema},
    :zen/desc "Access to on-line information"},
   :physicalCharacteristics
   {:confirms #{hl7-fhir-r4-core.ProdCharacteristic/schema},
    :zen/desc "Dimensions, color etc."}}}}