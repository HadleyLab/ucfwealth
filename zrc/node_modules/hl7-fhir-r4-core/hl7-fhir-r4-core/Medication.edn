{ns hl7-fhir-r4-core.Medication,
 import
 #{hl7-fhir-r4-core.Substance
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.Ratio
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.value-set.medication-status
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Medication
   hl7-fhir-r4-core.Organization},
 schema
 {:zen/tags #{zen/schema zen.fhir/base-schema},
  :zen/desc
  "This resource is primarily used for the identification and definition of a medication for the purposes of prescribing, dispensing, and administering a medication as well as for making statements about medication use.",
  :zen.fhir/type "Medication",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Medication",
  :zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :type zen/map,
  :keys
  {:amount
   {:confirms #{hl7-fhir-r4-core.Ratio/schema},
    :fhir/flags #{:SU},
    :zen/desc "Amount of drug in package"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :batch
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:lotNumber
     {:confirms #{hl7-fhir-r4-core.string/schema},
      :zen/desc "Identifier assigned to batch"},
     :_lotNumber {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :expirationDate
     {:confirms #{hl7-fhir-r4-core.dateTime/schema},
      :zen/desc "When batch will expire"},
     :_expirationDate {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :zen/desc "Details about packaged medications"},
   :ingredient
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:item
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:CodeableConcept :Reference}},
       :type zen/map,
       :keys
       {:CodeableConcept
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
        :Reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference
         {:refers
          #{hl7-fhir-r4-core.Medication/schema
            hl7-fhir-r4-core.Substance/schema}}}},
       :zen/desc "The actual ingredient or content"},
      :isActive
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :zen/desc "Active ingredient indicator"},
      :_isActive {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :strength
      {:confirms #{hl7-fhir-r4-core.Ratio/schema},
       :zen/desc "Quantity of ingredient present"}},
     :require #{:item},
     :zen/desc "Active or inactive ingredient"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.medication-status/value-set,
     :strength :required},
    :zen/desc "active | inactive | entered-in-error"},
   :code
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Codes that identify this medication"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Business identifier for this medication"}},
   :manufacturer
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Manufacturer of the item"},
   :form
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "powder | tablets | capsule +"}}}}