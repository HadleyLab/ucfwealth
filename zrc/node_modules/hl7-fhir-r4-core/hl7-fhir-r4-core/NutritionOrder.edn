{ns hl7-fhir-r4-core.NutritionOrder,
 import
 #{hl7-fhir-r4-core.value-set.request-status
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.SimpleQuantity
   hl7-fhir-r4-core.Ratio
   hl7-fhir-r4-core.canonical
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.Timing
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.AllergyIntolerance
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.value-set.request-intent
   hl7-fhir-r4-core.Identifier},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/NutritionOrder",
  :require #{:patient :dateTime :status :intent},
  :type zen/map,
  :zen/desc
  "A request to supply a diet, formula feeding (enteral) or oral nutritional supplement to a patient/resident.",
  :keys
  {:patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc
    "The person who requires the diet, formula or nutritional supplement"},
   :oralDiet
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:type
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "Type of oral diet or diet restrictions that describe what can be consumed orally"}},
     :schedule
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.Timing/schema},
       :zen/desc "Scheduled frequency of diet"}},
     :nutrient
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
       :type zen/map,
       :keys
       {:modifier
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
         :zen/desc "Type of nutrient that is being modified"},
        :amount
        {:confirms
         #{hl7-fhir-r4-core.Quantity/schema
           hl7-fhir-r4-core.SimpleQuantity/schema},
         :zen/desc "Quantity of the specified nutrient"}},
       :zen/desc "Required  nutrient modifications"}},
     :texture
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
       :type zen/map,
       :keys
       {:modifier
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
         :zen/desc
         "Code to indicate how to alter the texture of the foods, e.g. pureed"},
        :foodType
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
         :zen/desc
         "Concepts that are used to identify an entity that is ingested for nutritional purposes"}},
       :zen/desc "Required  texture modifications"}},
     :fluidConsistencyType
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc
       "The required consistency of fluids and liquids provided to the patient"}},
     :instruction
     {:confirms #{hl7-fhir-r4-core.string/schema},
      :fhir/flags #{:SU},
      :zen/desc
      "Instructions or additional information about the oral diet"},
     :_instruction {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :zen/desc "Oral diet components"},
   :instantiatesCanonical
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.canonical/schema},
     :fhir/flags #{:SU},
     :zen/desc "Instantiates FHIR protocol or definition"}},
   :instantiatesUri
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.uri/schema},
     :fhir/flags #{:SU},
     :zen/desc "Instantiates external protocol or definition"}},
   :instantiates
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.uri/schema},
     :zen/desc "Instantiates protocol or definition"}},
   :encounter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "The encounter associated with this nutrition order"},
   :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_instantiates
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Comments"}},
   :dateTime
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "Date and time the nutrition order was requested"},
   :enteralFormula
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:_administrationInstruction
     {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :additiveType
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
      :zen/desc "Type of modular component to add to the feeding"},
     :_baseFormulaProductName
     {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :maxVolumeToDeliver
     {:confirms
      #{hl7-fhir-r4-core.Quantity/schema
        hl7-fhir-r4-core.SimpleQuantity/schema},
      :zen/desc "Upper limit on formula volume per unit of time"},
     :baseFormulaType
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
      :fhir/flags #{:SU},
      :zen/desc "Type of enteral or infant formula"},
     :routeofAdministration
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
      :zen/desc
      "How the formula should enter the patient's gastrointestinal tract"},
     :additiveProductName
     {:confirms #{hl7-fhir-r4-core.string/schema},
      :zen/desc "Product or brand name of the modular additive"},
     :caloricDensity
     {:confirms
      #{hl7-fhir-r4-core.Quantity/schema
        hl7-fhir-r4-core.SimpleQuantity/schema},
      :zen/desc
      "Amount of energy per specified volume that is required"},
     :administrationInstruction
     {:confirms #{hl7-fhir-r4-core.string/schema},
      :fhir/flags #{:SU},
      :zen/desc "Formula feeding instructions expressed as text"},
     :administration
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
       :type zen/map,
       :keys
       {:schedule
        {:confirms #{hl7-fhir-r4-core.Timing/schema},
         :zen/desc "Scheduled frequency of enteral feeding"},
        :quantity
        {:confirms
         #{hl7-fhir-r4-core.Quantity/schema
           hl7-fhir-r4-core.SimpleQuantity/schema},
         :zen/desc "The volume of formula to provide"},
        :rate
        {:fhir/polymorphic true,
         :exclusive-keys #{#{:Ratio :Quantity}},
         :type zen/map,
         :keys
         {:Quantity {:confirms #{hl7-fhir-r4-core.Quantity/schema}},
          :Ratio {:confirms #{hl7-fhir-r4-core.Ratio/schema}}},
         :zen/desc
         "Speed with which the formula is provided per period of time"}},
       :zen/desc "Formula feeding instruction as structured data"}},
     :_additiveProductName
     {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :baseFormulaProductName
     {:confirms #{hl7-fhir-r4-core.string/schema},
      :zen/desc
      "Product or brand name of the enteral or infant formula"}},
    :zen/desc "Enteral formula components"},
   :_intent {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :foodPreferenceModifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc
     "Order-specific modifier about the type of food that should be given"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.request-status/value-set,
     :strength :required},
    :zen/desc
    "draft | active | on-hold | revoked | completed | entered-in-error | unknown"},
   :excludeFoodModifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc
     "Order-specific modifier about the type of food that should not be given"}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc "Identifiers assigned to this order"}},
   :intent
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.request-intent/value-set,
     :strength :required},
    :zen/desc
    "proposal | plan | directive | order | original-order | reflex-order | filler-order | instance-order | option"},
   :orderer
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc
    "Who ordered the diet, formula or nutritional supplement"},
   :supplement
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "Type of supplement product requested"},
      :productName
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc
       "Product or brand name of the nutritional supplement"},
      :_productName {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :schedule
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.Timing/schema},
        :zen/desc "Scheduled frequency of supplement"}},
      :quantity
      {:confirms
       #{hl7-fhir-r4-core.Quantity/schema
         hl7-fhir-r4-core.SimpleQuantity/schema},
       :zen/desc "Amount of the nutritional supplement"},
      :instruction
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "Instructions or additional information about the oral supplement"},
      :_instruction {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :zen/desc "Supplement components"}},
   :_instantiatesCanonical
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :allergyIntolerance
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.AllergyIntolerance/schema}},
     :zen/desc
     "List of the patient's food and nutrition-related allergies and intolerances"}},
   :_instantiatesUri
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}}},
  :zen.fhir/type "NutritionOrder"}}