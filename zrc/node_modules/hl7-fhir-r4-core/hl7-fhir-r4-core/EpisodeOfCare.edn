{ns hl7-fhir-r4-core.EpisodeOfCare,
 import
 #{hl7-fhir-r4-core.CareTeam
   hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.Condition
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.value-set.diagnosis-role
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.value-set.episode-of-care-status
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.positiveInt
   hl7-fhir-r4-core.ServiceRequest
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Account
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/EpisodeOfCare",
  :require #{:patient :status},
  :type zen/map,
  :zen/desc
  "An association between a patient and an organization / healthcare provider(s) during which time encounters may occur. The managing organization assumes a level of responsibility for the patient during this time.",
  :keys
  {:patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc "The patient who is the focus of this episode of care"},
   :diagnosis
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:condition
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :fhir/flags #{:SU},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.Condition/schema}},
       :zen/desc
       "Conditions/problems/diagnoses this episode of care is for"},
      :role
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen.fhir/value-set
       {:symbol hl7-fhir-r4-core.value-set.diagnosis-role/value-set,
        :strength :preferred},
       :zen/desc
       "Role that this diagnosis has within the episode of care (e.g. admission, billing, discharge â€¦)"},
      :rank
      {:confirms #{hl7-fhir-r4-core.positiveInt/schema},
       :fhir/flags #{:SU},
       :zen/desc "Ranking of the diagnosis (for each role type)"},
      :_rank {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:condition},
     :fhir/flags #{:SU},
     :zen/desc
     "The list of diagnosis relevant to this episode of care"}},
   :managingOrganization
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Organization that assumes care"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :type
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Type/class  - e.g. specialist referral, disease management"}},
   :account
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Account/schema}},
     :zen/desc
     "The set of accounts that may be used for billing for this EpisodeOfCare"}},
   :referralRequest
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.ServiceRequest/schema}},
     :zen/desc "Originating Referral Request(s)"}},
   :team
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference {:refers #{hl7-fhir-r4-core.CareTeam/schema}},
     :zen/desc
     "Other practitioners facilitating this episode of care"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.episode-of-care-status/value-set,
     :strength :required},
    :zen/desc
    "planned | waitlist | active | onhold | finished | cancelled | entered-in-error"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc
     "Business Identifier(s) relevant for this EpisodeOfCare"}},
   :period
   {:confirms #{hl7-fhir-r4-core.Period/schema},
    :fhir/flags #{:SU},
    :zen/desc "Interval during responsibility is assumed"},
   :careManager
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc "Care manager/care coordinator for the patient"},
   :statusHistory
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:status
      {:confirms #{hl7-fhir-r4-core.code/schema},
       :zen.fhir/value-set
       {:symbol
        hl7-fhir-r4-core.value-set.episode-of-care-status/value-set,
        :strength :required},
       :zen/desc
       "planned | waitlist | active | onhold | finished | cancelled | entered-in-error"},
      :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :period
      {:confirms #{hl7-fhir-r4-core.Period/schema},
       :zen/desc
       "Duration the EpisodeOfCare was in the specified status"}},
     :require #{:status :period},
     :zen/desc
     "Past list of status codes (the current status may be included to cover the start date of the status)"}}},
  :zen.fhir/type "EpisodeOfCare"}}