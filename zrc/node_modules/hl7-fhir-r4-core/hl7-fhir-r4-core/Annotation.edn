{ns hl7-fhir-r4-core.Annotation,
 import
 #{hl7-fhir-r4-core.markdown
   zen.fhir
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms #{hl7-fhir-r4-core.Element/schema},
  :zen/tags #{zen/schema zen.fhir/structure-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Annotation",
  :require #{:text},
  :type zen/map,
  :zen/desc
  "Base StructureDefinition for Annotation Type: A  text note which also  contains information about who made the statement and when.",
  :keys
  {:author
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:string :Reference}},
    :type zen/map,
    :keys
    {:Reference
     {:confirms
      #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
      :zen.fhir/reference
      {:refers
       #{hl7-fhir-r4-core.Patient/schema
         hl7-fhir-r4-core.Organization/schema
         hl7-fhir-r4-core.Practitioner/schema
         hl7-fhir-r4-core.RelatedPerson/schema}}},
     :string {:confirms #{hl7-fhir-r4-core.string/schema}},
     :_string {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :fhir/flags #{:SU},
    :zen/desc "Individual responsible for the annotation"},
   :time
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "When the annotation was made"},
   :_time {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :text
   {:confirms #{hl7-fhir-r4-core.markdown/schema},
    :fhir/flags #{:SU},
    :zen/desc "The annotation  - text content (as markdown)"},
   :_text {:confirms #{hl7-fhir-r4-core.Element/schema}}},
  :zen.fhir/type "Annotation"}}