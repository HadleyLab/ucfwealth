{ns hl7-fhir-r4-core.OrganizationAffiliation,
 import
 #{hl7-fhir-r4-core.HealthcareService
   hl7-fhir-r4-core.Period
   zen.fhir
   hl7-fhir-r4-core.ContactPoint
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.value-set.c80-practice-codes
   hl7-fhir-r4-core.Endpoint
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Location
   hl7-fhir-r4-core.Organization},
 schema
 {:zen/tags #{zen/schema zen.fhir/base-schema},
  :zen/desc
  "Defines an affiliation/assotiation/relationship between 2 distinct oganizations, that is not a part-of relationship/sub-division relationship.",
  :zen.fhir/type "OrganizationAffiliation",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/OrganizationAffiliation",
  :zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :type zen/map,
  :keys
  {:_active {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :specialty
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen.fhir/value-set
     {:symbol hl7-fhir-r4-core.value-set.c80-practice-codes/value-set,
      :strength :preferred},
     :zen/desc
     "Specific specialty of the participatingOrganization in the context of the role"}},
   :organization
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Organization where the role is available"},
   :participatingOrganization
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc
    "Organization that provides/performs the role (e.g. providing services or is a member of)"},
   :active
   {:confirms #{hl7-fhir-r4-core.boolean/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "Whether this organization affiliation record is in active use"},
   :code
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Definition of the role the participatingOrganization plays"}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Business identifiers that are specific to this role"}},
   :telecom
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactPoint/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Contact details at the participatingOrganization relevant to this Affiliation"}},
   :network
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Organization/schema}},
     :zen/desc
     "Health insurance provider network in which the participatingOrganization provides the role's services (if defined) at the indicated locations (if defined)"}},
   :period
   {:confirms #{hl7-fhir-r4-core.Period/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The period during which the participatingOrganization is affiliated with the primary organization"},
   :location
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Location/schema}},
     :zen/desc "The location(s) at which the role occurs"}},
   :endpoint
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Endpoint/schema}},
     :zen/desc
     "Technical endpoints providing access to services operated for this role"}},
   :healthcareService
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.HealthcareService/schema}},
     :zen/desc "Healthcare services provided through the role"}}}}}