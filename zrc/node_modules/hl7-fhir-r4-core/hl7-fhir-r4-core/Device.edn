{ns hl7-fhir-r4-core.Device,
 import
 #{hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.value-set.device-nametype
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.ContactPoint
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.value-set.device-status
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.DeviceDefinition
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.base64Binary
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.value-set.udi-entry-type
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Location
   hl7-fhir-r4-core.Organization},
 schema
 {:zen/tags #{zen/schema zen.fhir/base-schema},
  :zen/desc
  "A type of a manufactured item that is used in the provision of healthcare without being substantially changed through that activity. The device may be a medical or non-medical device.",
  :zen.fhir/type "Device",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Device",
  :zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :type zen/map,
  :keys
  {:patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc "Patient to whom Device is affixed"},
   :definition
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.DeviceDefinition/schema}},
    :zen/desc "The reference to the definition for the device"},
   :serialNumber
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Serial number assigned by the manufacturer"},
   :parent
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Device/schema}},
    :zen/desc "The parent device"},
   :deviceName
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:name
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "The name of the device"},
      :_name {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :type
      {:confirms #{hl7-fhir-r4-core.code/schema},
       :zen.fhir/value-set
       {:symbol hl7-fhir-r4-core.value-set.device-nametype/value-set,
        :strength :required},
       :zen/desc
       "udi-label-name | user-friendly-name | patient-reported-name | manufacturer-name | model-name | other"},
      :_type {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:name :type},
     :zen/desc "The name of the device as given by the manufacturer"}},
   :_partNumber {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :property
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc
       "Code that specifies the property DeviceDefinitionPropetyCode (Extensible)"},
      :valueQuantity
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.Quantity/schema},
        :zen/desc "Property value as a quantity"}},
      :valueCode
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :zen/desc
        "Property value as a code, e.g., NTP4 (synced to NTP)"}}},
     :require #{:type},
     :zen/desc
     "The actual configuration settings of a device as it actually operates, e.g., regulation status, time properties"}},
   :_modelNumber {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :partNumber
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "The part number of the device"},
   :modelNumber
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "The model number for the device"},
   :_manufactureDate {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :type
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "The kind or type of device"},
   :statusReason
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc
     "online | paused | standby | offline | not-ready | transduc-discon | hw-discon | off"}},
   :specialization
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:systemType
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc
       "The standard that is used to operate and communicate"},
      :version
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc
       "The version of the standard that is used to operate and communicate"},
      :_version {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:systemType},
     :zen/desc
     "The capabilities supported on a  device, the standards to which the device conforms for a particular purpose, and used for the communication"}},
   :_expirationDate {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :distinctIdentifier
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "The distinct identification string"},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Device notes and comments"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.device-status/value-set,
     :strength :required},
    :zen/desc "active | inactive | entered-in-error | unknown"},
   :safety
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Safety Characteristics of Device"}},
   :lotNumber
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Lot number of manufacture"},
   :url
   {:confirms #{hl7-fhir-r4-core.uri/schema},
    :zen/desc "Network address to contact device"},
   :_distinctIdentifier {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc "Instance identifier"}},
   :manufacturer
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Name of device manufacturer"},
   :_manufacturer {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_serialNumber {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :version
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "The type of the device version"},
      :component
      {:confirms #{hl7-fhir-r4-core.Identifier/schema},
       :zen/desc "A single component of the device version"},
      :value
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "The version text"},
      :_value {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:value},
     :zen/desc
     "The actual design of the device or software version running on the device"}},
   :_lotNumber {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :location
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Location/schema}},
    :zen/desc "Where the device is found"},
   :contact
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactPoint/schema},
     :zen/desc "Details for human/organization for support"}},
   :manufactureDate
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc "Date when the device was made"},
   :owner
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Organization responsible for device"},
   :_url {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :expirationDate
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc
    "Date and time of expiry of this device (if applicable)"},
   :udiCarrier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:_jurisdiction {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :deviceIdentifier
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :fhir/flags #{:SU},
       :zen/desc "Mandatory fixed portion of UDI"},
      :_entryType {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :jurisdiction
      {:confirms #{hl7-fhir-r4-core.uri/schema},
       :zen/desc "Regional UDI authority"},
      :entryType
      {:confirms #{hl7-fhir-r4-core.code/schema},
       :zen.fhir/value-set
       {:symbol hl7-fhir-r4-core.value-set.udi-entry-type/value-set,
        :strength :required},
       :zen/desc "barcode | rfid | manual +"},
      :issuer
      {:confirms #{hl7-fhir-r4-core.uri/schema},
       :zen/desc "UDI Issuing Organization"},
      :_deviceIdentifier
      {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :carrierAIDC
      {:confirms #{hl7-fhir-r4-core.base64Binary/schema},
       :fhir/flags #{:SU},
       :zen/desc "UDI Machine Readable Barcode String"},
      :_carrierHRF {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :_carrierAIDC {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :_issuer {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :carrierHRF
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :fhir/flags #{:SU},
       :zen/desc "UDI Human Readable Barcode String"}},
     :fhir/flags #{:SU},
     :zen/desc "Unique Device Identifier (UDI) Barcode string"}}}}}