{ns hl7-fhir-r4-core.DetectedIssue,
 import
 #{hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.value-set.detectedissue-category
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.value-set.observation-status
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.value-set.detectedissue-mitigation-action
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.value-set.detectedissue-severity},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/DetectedIssue",
  :require #{:status},
  :type zen/map,
  :zen/desc
  "Indicates an actual or potential clinical issue with or between one or more active or proposed clinical actions for a patient; e.g. Drug-drug interaction, Ineffective treatment frequency, Procedure-condition conflict, etc.",
  :keys
  {:patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc "Associated patient"},
   :_reference {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :evidence
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:code
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :zen/desc "Manifestation"}},
      :detail
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
        :zen.fhir/reference {:refers #{}},
        :zen/desc "Supporting information"}}},
     :zen/desc "Supporting evidence"}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :mitigation
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:action
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen.fhir/value-set
       {:symbol
        hl7-fhir-r4-core.value-set.detectedissue-mitigation-action/value-set,
        :strength :preferred},
       :zen/desc "What mitigation?"},
      :date
      {:confirms #{hl7-fhir-r4-core.dateTime/schema},
       :zen/desc "Date committed"},
      :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :author
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.PractitionerRole/schema
          hl7-fhir-r4-core.Practitioner/schema}},
       :zen/desc "Who is committing?"}},
     :require #{:action},
     :zen/desc "Step taken to address"}},
   :author
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Device/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc "The provider or device that identified the issue"},
   :reference
   {:confirms #{hl7-fhir-r4-core.uri/schema},
    :zen/desc "Authority for issue"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.observation-status/value-set,
     :strength :required},
    :zen/desc "registered | preliminary | final | amended +"},
   :severity
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.detectedissue-severity/value-set,
     :strength :required},
    :zen/desc "high | moderate | low"},
   :code
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.detectedissue-category/value-set,
     :strength :preferred},
    :zen/desc
    "Issue Category, e.g. drug-drug, duplicate therapy, etc."},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Unique id for the detected issue"}},
   :_severity {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :identified
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:dateTime :Period}},
    :type zen/map,
    :keys
    {:dateTime {:confirms #{hl7-fhir-r4-core.dateTime/schema}},
     :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :Period {:confirms #{hl7-fhir-r4-core.Period/schema}}},
    :fhir/flags #{:SU},
    :zen/desc "When identified"},
   :implicated
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference {:refers #{}},
     :zen/desc "Problem resource"}},
   :_detail {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :detail
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Description and context"}},
  :zen.fhir/type "DetectedIssue"}}