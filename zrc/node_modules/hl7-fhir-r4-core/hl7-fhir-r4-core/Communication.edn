{ns hl7-fhir-r4-core.Communication,
 import
 #{hl7-fhir-r4-core.CareTeam
   hl7-fhir-r4-core.HealthcareService
   hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.Observation
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.Condition
   hl7-fhir-r4-core.canonical
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.DiagnosticReport
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.value-set.request-priority
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Communication
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.value-set.event-status
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Attachment
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.DocumentReference
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Communication",
  :require #{:status},
  :type zen/map,
  :zen/desc
  "An occurrence of information being transmitted; e.g. an alert that was sent to a responsible provider, a public health agency that was notified about a reportable condition.",
  :keys
  {:category
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Message category"}},
   :received
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc "When received"},
   :instantiatesCanonical
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.canonical/schema},
     :fhir/flags #{:SU},
     :zen/desc "Instantiates FHIR protocol or definition"}},
   :payload
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:content
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:string :Attachment :Reference}},
       :type zen/map,
       :keys
       {:string {:confirms #{hl7-fhir-r4-core.string/schema}},
        :_string {:confirms #{hl7-fhir-r4-core.Element/schema}},
        :Attachment {:confirms #{hl7-fhir-r4-core.Attachment/schema}},
        :Reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference {:refers #{}}}},
       :zen/desc "Message part content"}},
     :require #{:content},
     :zen/desc "Message payload"}},
   :instantiatesUri
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.uri/schema},
     :fhir/flags #{:SU},
     :zen/desc "Instantiates external protocol or definition"}},
   :_received {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :encounter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "Encounter created as part of"},
   :medium
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "A channel of communication"}},
   :_priority {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :recipient
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.CareTeam/schema
        hl7-fhir-r4-core.Patient/schema
        hl7-fhir-r4-core.PractitionerRole/schema
        hl7-fhir-r4-core.HealthcareService/schema
        hl7-fhir-r4-core.Organization/schema
        hl7-fhir-r4-core.Device/schema
        hl7-fhir-r4-core.Practitioner/schema
        hl7-fhir-r4-core.RelatedPerson/schema
        hl7-fhir-r4-core.Group/schema}},
     :zen/desc "Message recipient"}},
   :reasonCode
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Indication for message"}},
   :statusReason
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Reason for current status"},
   :topic
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "Description of the purpose/content"},
   :sent
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc "When sent"},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Comments made about the communication"}},
   :priority
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.request-priority/value-set,
     :strength :required},
    :zen/desc "routine | urgent | asap | stat"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.event-status/value-set,
     :strength :required},
    :zen/desc
    "preparation | in-progress | not-done | on-hold | stopped | completed | entered-in-error | unknown"},
   :sender
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema
       hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.HealthcareService/schema
       hl7-fhir-r4-core.Organization/schema
       hl7-fhir-r4-core.Device/schema
       hl7-fhir-r4-core.Practitioner/schema
       hl7-fhir-r4-core.RelatedPerson/schema}},
    :zen/desc "Message sender"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Unique identifier"}},
   :inResponseTo
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Communication/schema}},
     :zen/desc "Reply to"}},
   :basedOn
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference {:refers #{}},
     :zen/desc "Request fulfilled by this communication"}},
   :partOf
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference {:refers #{}},
     :zen/desc "Part of this action"}},
   :_instantiatesCanonical
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Focus of message"},
   :about
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference {:refers #{}},
     :zen/desc "Resources that pertain to this communication"}},
   :reasonReference
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Observation/schema
        hl7-fhir-r4-core.DocumentReference/schema
        hl7-fhir-r4-core.DiagnosticReport/schema
        hl7-fhir-r4-core.Condition/schema}},
     :zen/desc "Why was communication done?"}},
   :_instantiatesUri
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :_sent {:confirms #{hl7-fhir-r4-core.Element/schema}}},
  :zen.fhir/type "Communication"}}