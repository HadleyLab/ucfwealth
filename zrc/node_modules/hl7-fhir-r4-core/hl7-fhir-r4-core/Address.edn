{ns hl7-fhir-r4-core.Address,
 import
 #{hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.value-set.address-use
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.value-set.address-type},
 schema
 {:zen/tags #{zen/schema zen.fhir/structure-schema},
  :zen/desc
  "Base StructureDefinition for Address Type: An address expressed using postal conventions (as opposed to GPS or other location definition formats).  This data type may be used to convey addresses for use in delivering mail as well as for visiting locations which might not be valid for mail delivery.  There are a variety of postal address formats defined around the world.",
  :zen.fhir/type "Address",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Address",
  :zen.fhir/version "0.5.18",
  :confirms #{hl7-fhir-r4-core.Element/schema},
  :type zen/map,
  :keys
  {:_line
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :use
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.address-use/value-set,
     :strength :required},
    :zen/desc
    "home | work | temp | old | billing - purpose of this address"},
   :city
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Name of city, town etc."},
   :_type {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :type
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.address-type/value-set,
     :strength :required},
    :zen/desc "postal | physical | both"},
   :_city {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :state
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Sub-unit of country (abbreviations ok)"},
   :_district {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_state {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :line
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.string/schema},
     :fhir/flags #{:SU},
     :zen/desc "Street name, number, direction & P.O. Box etc."}},
   :postalCode
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Postal code for area"},
   :_country {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_postalCode {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_text {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :period
   {:confirms #{hl7-fhir-r4-core.Period/schema},
    :fhir/flags #{:SU},
    :zen/desc "Time period when address was/is in use"},
   :country
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Country (e.g. can be ISO 3166 2 or 3 letter code)"},
   :_use {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :district
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "District name (aka county)"},
   :text
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Text representation of the address"}}}}