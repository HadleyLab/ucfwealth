           :value
           {:confirms #{hl7-fhir-r4-core.string/schema},
            :fhir/flags #{:SU},
            :zen/desc
            "Code from the system, or regex criteria, or boolean value for exists"},
           :_value {:confirms #{hl7-fhir-r4-core.Element/schema}}},
          :require #{:property :value :op},
          :fhir/flags #{:SU},
          :zen/desc
          "Select codes/concepts by their properties (including relationships)"}},
        :valueSet
        {:type zen/vector,
         :every
         {:confirms #{hl7-fhir-r4-core.canonical/schema},
          :fhir/flags #{:SU},
          :zen/desc "Select the contents included in this value set"}},
        :_valueSet
        {:type zen/vector,
         :every {:confirms #{hl7-fhir-r4-core.Element/schema}}}},
       :fhir/flags #{:SU},
       :zen/desc
       "Include one or more codes from a code system or other value set(s)"},
      :minItems 1},
     :exclude
     {:type zen/vector,
      :every
      {:zen/desc
       "Explicitly exclude codes from a code system or other value sets"}}},
    :require #{:include},
    :zen/desc "Content logical definition of the value set (CLD)"},
   :date
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "Date last changed"},
   :publisher
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Name of the publisher (organization or individual)"},
