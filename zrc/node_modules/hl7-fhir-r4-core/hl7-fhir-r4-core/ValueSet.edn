          :keys
          {:code
           {:confirms #{hl7-fhir-r4-core.code/schema},
            :zen/desc "Code or expression from system"},
           :_code {:confirms #{hl7-fhir-r4-core.Element/schema}},
           :display
           {:confirms #{hl7-fhir-r4-core.string/schema},
            :zen/desc
            "Text to display for this code for this value set in this valueset"},
           :_display {:confirms #{hl7-fhir-r4-core.Element/schema}},
           :designation
           {:type zen/vector,
            :every
            {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
             :type zen/map,
             :keys
             {:language
              {:confirms #{hl7-fhir-r4-core.code/schema},
               :zen.fhir/value-set
               {:symbol hl7-fhir-r4-core.value-set.languages/value-set,
                :strength :preferred},
               :zen/desc "Human language of the designation"},
              :_language
              {:confirms #{hl7-fhir-r4-core.Element/schema}},
              :use
              {:confirms #{hl7-fhir-r4-core.Coding/schema},
               :zen/desc "Types of uses of designations"},
              :value
              {:confirms #{hl7-fhir-r4-core.string/schema},
               :zen/desc "The text value for this designation"},
              :_value {:confirms #{hl7-fhir-r4-core.Element/schema}}},
             :require #{:value},
             :zen/desc
             "Additional representations for this concept"}}},
          :require #{:code},
          :zen/desc "A concept defined in the system"}},
        :filter
        {:type zen/vector,
         :every
         {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
          :type zen/map,
          :keys
          {:property
           {:confirms #{hl7-fhir-r4-core.code/schema},
            :fhir/flags #{:SU},
            :zen/desc "A property/filter defined by the code system"},
           :_property {:confirms #{hl7-fhir-r4-core.Element/schema}},
           :op
           {:confirms #{hl7-fhir-r4-core.code/schema},
            :fhir/flags #{:SU},
            :zen.fhir/value-set
            {:symbol
             hl7-fhir-r4-core.value-set.filter-operator/value-set,
             :strength :required},
            :zen/desc
            "= | is-a | descendent-of | is-not-a | regex | in | not-in | generalizes | exists"},
