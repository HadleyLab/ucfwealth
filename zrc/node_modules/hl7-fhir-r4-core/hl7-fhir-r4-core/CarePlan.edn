     :zen.fhir/reference {:refers #{hl7-fhir-r4-core.CarePlan/schema}},
     :zen/desc "CarePlan replaced by this CarePlan"}},
   :period
   {:confirms #{hl7-fhir-r4-core.Period/schema},
    :fhir/flags #{:SU},
    :zen/desc "Time period plan covers"},
