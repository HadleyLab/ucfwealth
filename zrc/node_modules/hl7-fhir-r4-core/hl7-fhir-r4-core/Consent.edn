{ns hl7-fhir-r4-core.Consent,
 import
 #{hl7-fhir-r4-core.CareTeam
   hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.value-set.consent-data-meaning
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.value-set.consent-provision-type
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.Contract
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.value-set.consent-state-codes
   hl7-fhir-r4-core.Coding
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Consent
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.QuestionnaireResponse
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Attachment
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.DocumentReference
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Consent",
  :require #{:category :scope :status},
  :type zen/map,
  :zen/desc
  "A record of a healthcare consumerâ€™s  choices, which permits or denies identified recipient(s) or recipient role(s) to perform one or more actions within a given policy context, for specific purposes and periods of time.",
  :keys
  {:patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc "Who the consent applies to"},
   :category
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Classification of the consent statement - for indexing/retrieval"},
    :minItems 1},
   :provision
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:provision
     {:type zen/vector, :every {:zen/desc "Nested Exception Rules"}},
     :purpose
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.Coding/schema},
       :fhir/flags #{:SU},
       :zen/desc "Context of activities covered by this rule"}},
     :dataPeriod
     {:confirms #{hl7-fhir-r4-core.Period/schema},
      :fhir/flags #{:SU},
      :zen/desc "Timeframe for data controlled by this rule"},
     :_type {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :type
     {:confirms #{hl7-fhir-r4-core.code/schema},
      :fhir/flags #{:SU},
      :zen.fhir/value-set
      {:symbol
       hl7-fhir-r4-core.value-set.consent-provision-type/value-set,
       :strength :required},
      :zen/desc "deny | permit"},
     :class
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.Coding/schema},
       :fhir/flags #{:SU},
       :zen/desc "e.g. Resource Type, Profile, CDA, etc."}},
     :code
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "e.g. LOINC or SNOMED CT code, etc. in the content"}},
     :action
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "Actions controlled by this rule"}},
     :period
     {:confirms #{hl7-fhir-r4-core.Period/schema},
      :fhir/flags #{:SU},
      :zen/desc "Timeframe for this rule"},
     :securityLabel
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.Coding/schema},
       :fhir/flags #{:SU},
       :zen/desc "Security Labels that define affected resources"}},
     :actor
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
       :type zen/map,
       :keys
       {:role
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
         :zen/desc "How the actor is involved"},
        :reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference
         {:refers
          #{hl7-fhir-r4-core.CareTeam/schema
            hl7-fhir-r4-core.Patient/schema
            hl7-fhir-r4-core.PractitionerRole/schema
            hl7-fhir-r4-core.Organization/schema
            hl7-fhir-r4-core.Device/schema
            hl7-fhir-r4-core.Practitioner/schema
            hl7-fhir-r4-core.RelatedPerson/schema
            hl7-fhir-r4-core.Group/schema}},
         :zen/desc "Resource for the actor (or group, by role)"}},
       :require #{:role :reference},
       :zen/desc
       "Who|what controlled by this rule (or group, by role)"}},
     :data
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
       :type zen/map,
       :keys
       {:meaning
        {:confirms #{hl7-fhir-r4-core.code/schema},
         :fhir/flags #{:SU},
         :zen.fhir/value-set
         {:symbol
          hl7-fhir-r4-core.value-set.consent-data-meaning/value-set,
          :strength :required},
         :zen/desc "instance | related | dependents | authoredby"},
        :_meaning {:confirms #{hl7-fhir-r4-core.Element/schema}},
        :reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :fhir/flags #{:SU},
         :zen.fhir/reference {:refers #{}},
         :zen/desc "The actual data reference"}},
       :require #{:reference :meaning},
       :fhir/flags #{:SU},
       :zen/desc "Data controlled by this rule"}}},
    :fhir/flags #{:SU},
    :zen/desc "Constraints to the base Consent.policyRule"},
   :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :organization
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Organization/schema}},
     :zen/desc "Custodian of the consent"}},
   :verification
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:verified
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :fhir/flags #{:SU},
       :zen/desc "Has been verified"},
      :_verified {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :verifiedWith
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.Patient/schema
          hl7-fhir-r4-core.RelatedPerson/schema}},
       :zen/desc "Person who verified"},
      :verificationDate
      {:confirms #{hl7-fhir-r4-core.dateTime/schema},
       :zen/desc "When consent verified"},
      :_verificationDate
      {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:verified},
     :fhir/flags #{:SU},
     :zen/desc "Consent Verified by patient or family"}},
   :source
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:Attachment :Reference}},
    :type zen/map,
    :keys
    {:Attachment {:confirms #{hl7-fhir-r4-core.Attachment/schema}},
     :Reference
     {:confirms
      #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
      :zen.fhir/reference
      {:refers
       #{hl7-fhir-r4-core.QuestionnaireResponse/schema
         hl7-fhir-r4-core.Contract/schema
         hl7-fhir-r4-core.DocumentReference/schema
         hl7-fhir-r4-core.Consent/schema}}}},
    :fhir/flags #{:SU},
    :zen/desc "Source from which this consent is taken"},
   :scope
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU :?!},
    :zen/desc
    "Which of the four areas this resource covers (extensible)"},
   :policy
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:authority
      {:confirms #{hl7-fhir-r4-core.uri/schema},
       :zen/desc "Enforcement source for policy"},
      :_authority {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :uri
      {:confirms #{hl7-fhir-r4-core.uri/schema},
       :zen/desc "Specific policy covered by this consent"},
      :_uri {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :zen/desc "Policies covered by this consent"}},
   :dateTime
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "When this Consent was created or indexed"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.consent-state-codes/value-set,
     :strength :required},
    :zen/desc
    "draft | proposed | active | rejected | inactive | entered-in-error"},
   :policyRule
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Regulation that this consents to"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Identifier for this record (external references)"}},
   :performer
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Patient/schema
        hl7-fhir-r4-core.PractitionerRole/schema
        hl7-fhir-r4-core.Organization/schema
        hl7-fhir-r4-core.Practitioner/schema
        hl7-fhir-r4-core.RelatedPerson/schema}},
     :zen/desc "Who is agreeing to the policy and rules"}}},
  :zen.fhir/type "Consent"}}