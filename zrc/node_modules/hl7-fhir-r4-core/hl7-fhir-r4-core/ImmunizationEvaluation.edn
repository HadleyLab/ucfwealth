{ns hl7-fhir-r4-core.ImmunizationEvaluation,
 import
 #{hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.value-set.immunization-evaluation-status
   hl7-fhir-r4-core.Immunization
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.positiveInt
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/ImmunizationEvaluation",
  :require
  #{:patient :immunizationEvent :status :targetDisease :doseStatus},
  :type zen/map,
  :zen/desc
  "Describes a comparison of an immunization event against published recommendations to determine if the administration is \"valid\" in relation to those  recommendations.",
  :keys
  {:patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc "Who this evaluation is for"},
   :description
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Evaluation notes"},
   :date
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc "Date evaluation was performed"},
   :series
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Name of vaccine series"},
   :doseNumber
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:string :positiveInt}},
    :type zen/map,
    :keys
    {:positiveInt {:confirms #{hl7-fhir-r4-core.positiveInt/schema}},
     :_positiveInt {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :string {:confirms #{hl7-fhir-r4-core.string/schema}},
     :_string {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :zen/desc "Dose number within series"},
   :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :authority
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Who is responsible for publishing the recommendations"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_description {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :seriesDoses
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:string :positiveInt}},
    :type zen/map,
    :keys
    {:positiveInt {:confirms #{hl7-fhir-r4-core.positiveInt/schema}},
     :_positiveInt {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :string {:confirms #{hl7-fhir-r4-core.string/schema}},
     :_string {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :zen/desc "Recommended number of doses for immunity"},
   :doseStatusReason
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Reason for the dose status"}},
   :immunizationEvent
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Immunization/schema}},
    :zen/desc "Immunization being evaluated"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.immunization-evaluation-status/value-set,
     :strength :required},
    :zen/desc "completed | entered-in-error"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc "Business identifier"}},
   :targetDisease
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Evaluation target disease"},
   :doseStatus
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "Status of the dose relative to published recommendations"},
   :_series {:confirms #{hl7-fhir-r4-core.Element/schema}}},
  :zen.fhir/type "ImmunizationEvaluation"}}