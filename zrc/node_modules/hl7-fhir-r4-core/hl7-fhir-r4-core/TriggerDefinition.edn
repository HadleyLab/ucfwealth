{ns hl7-fhir-r4-core.TriggerDefinition,
 import
 #{hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.value-set.trigger-type
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.Timing
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.date
   hl7-fhir-r4-core.Schedule
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.DataRequirement
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.Expression},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms #{hl7-fhir-r4-core.Element/schema},
  :zen/tags #{zen/schema zen.fhir/structure-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/TriggerDefinition",
  :require #{:type},
  :type zen/map,
  :zen/desc
  "Base StructureDefinition for TriggerDefinition Type: A description of a triggering event. Triggering events can be named events, data events, or periodic, as determined by the type element.",
  :keys
  {:type
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.trigger-type/value-set,
     :strength :required},
    :zen/desc
    "named-event | periodic | data-changed | data-added | data-modified | data-removed | data-accessed | data-access-ended"},
   :_type {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :name
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Name or URI that identifies the event"},
   :_name {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :timing
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:date :dateTime :Timing :Reference}},
    :type zen/map,
    :keys
    {:Timing {:confirms #{hl7-fhir-r4-core.Timing/schema}},
     :Reference
     {:confirms
      #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
      :zen.fhir/reference
      {:refers #{hl7-fhir-r4-core.Schedule/schema}}},
     :date {:confirms #{hl7-fhir-r4-core.date/schema}},
     :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :dateTime {:confirms #{hl7-fhir-r4-core.dateTime/schema}},
     :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :fhir/flags #{:SU},
    :zen/desc "Timing of the event"},
   :data
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.DataRequirement/schema},
     :fhir/flags #{:SU},
     :zen/desc "Triggering data of the event (multiple = 'and')"}},
   :condition
   {:confirms #{hl7-fhir-r4-core.Expression/schema},
    :fhir/flags #{:SU},
    :zen/desc "Whether the event triggers (boolean expression)"}},
  :zen.fhir/type "TriggerDefinition"}}