{ns hl7-fhir-r4-core.MedicinalProductPharmaceutical,
 import
 #{hl7-fhir-r4-core.Duration
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.Ratio
   zen.fhir
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.DeviceDefinition
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.MedicinalProductIngredient
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/MedicinalProductPharmaceutical",
  :require #{:routeOfAdministration :administrableDoseForm},
  :type zen/map,
  :zen/desc
  "A pharmaceutical product described in terms of its composition and dose form.",
  :keys
  {:identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "An identifier for the pharmaceutical medicinal product"}},
   :administrableDoseForm
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The administrable dose form, after necessary reconstitution"},
   :unitOfPresentation
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Todo"},
   :ingredient
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.MedicinalProductIngredient/schema}},
     :zen/desc "Ingredient"}},
   :device
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.DeviceDefinition/schema}},
     :zen/desc "Accompanying device"}},
   :characteristics
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:code
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "A coded characteristic"},
      :status
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The status of characteristic e.g. assigned or pending"}},
     :require #{:code},
     :fhir/flags #{:SU},
     :zen/desc "Characteristics e.g. a products onset of action"}},
   :routeOfAdministration
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:code
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "Coded expression for the route"},
      :firstDose
      {:confirms #{hl7-fhir-r4-core.Quantity/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The first dose (dose quantity) administered in humans can be specified, for a product under investigation, using a numerical value and its unit of measurement"},
      :maxSingleDose
      {:confirms #{hl7-fhir-r4-core.Quantity/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The maximum single dose that can be administered as per the protocol of a clinical trial can be specified using a numerical value and its unit of measurement"},
      :maxDosePerDay
      {:confirms #{hl7-fhir-r4-core.Quantity/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The maximum dose per day (maximum dose quantity to be administered in any one 24-h period) that can be administered as per the protocol referenced in the clinical trial authorisation"},
      :maxDosePerTreatmentPeriod
      {:confirms #{hl7-fhir-r4-core.Ratio/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The maximum dose per treatment period that can be administered as per the protocol referenced in the clinical trial authorisation"},
      :maxTreatmentPeriod
      {:confirms #{hl7-fhir-r4-core.Duration/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The maximum treatment period during which an Investigational Medicinal Product can be administered as per the protocol referenced in the clinical trial authorisation"},
      :targetSpecies
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:code
         {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
          :fhir/flags #{:SU},
          :zen/desc "Coded expression for the species"},
         :withdrawalPeriod
         {:type zen/vector,
          :every
          {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
           :type zen/map,
           :keys
           {:tissue
            {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
             :fhir/flags #{:SU},
             :zen/desc
             "Coded expression for the type of tissue for which the withdrawal period applues, e.g. meat, milk"},
            :value
            {:confirms #{hl7-fhir-r4-core.Quantity/schema},
             :fhir/flags #{:SU},
             :zen/desc "A value for the time"},
            :supportingInformation
            {:confirms #{hl7-fhir-r4-core.string/schema},
             :fhir/flags #{:SU},
             :zen/desc
             "Extra information about the withdrawal period"},
            :_supportingInformation
            {:confirms #{hl7-fhir-r4-core.Element/schema}}},
           :require #{:tissue :value},
           :fhir/flags #{:SU},
           :zen/desc
           "A species specific time during which consumption of animal product is not appropriate"}}},
        :require #{:code},
        :fhir/flags #{:SU},
        :zen/desc "A species for which this route applies"}}},
     :require #{:code},
     :fhir/flags #{:SU},
     :zen/desc
     "The path by which the pharmaceutical product is taken into or makes contact with the body"},
    :minItems 1}},
  :zen.fhir/type "MedicinalProductPharmaceutical"}}