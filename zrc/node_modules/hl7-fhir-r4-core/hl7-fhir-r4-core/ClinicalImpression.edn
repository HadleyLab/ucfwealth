{ns hl7-fhir-r4-core.ClinicalImpression,
 import
 #{hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.Observation
   hl7-fhir-r4-core.ClinicalImpression
   hl7-fhir-r4-core.ImagingStudy
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.Condition
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.DiagnosticReport
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.QuestionnaireResponse
   hl7-fhir-r4-core.AllergyIntolerance
   hl7-fhir-r4-core.value-set.clinicalimpression-status
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Media
   hl7-fhir-r4-core.FamilyMemberHistory
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.RiskAssessment},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/ClinicalImpression",
  :require #{:status :subject},
  :type zen/map,
  :zen/desc
  "A record of a clinical assessment performed to determine what problem(s) may affect the patient and before planning the treatments or management strategies that are best to manage a patient's condition. Assessments are often 1:1 with a clinical consultation / encounter,  but this varies greatly depending on the clinical workflow. This resource is called \"ClinicalImpression\" rather than \"ClinicalAssessment\" to avoid confusion with the recording of assessment tools such as Apgar score.",
  :keys
  {:description
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Why/how the assessment was performed"},
   :date
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "When the assessment was documented"},
   :investigation
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:code
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "A name/code for the set"},
      :item
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
        :zen.fhir/reference
        {:refers
         #{hl7-fhir-r4-core.Media/schema
           hl7-fhir-r4-core.QuestionnaireResponse/schema
           hl7-fhir-r4-core.RiskAssessment/schema
           hl7-fhir-r4-core.ImagingStudy/schema
           hl7-fhir-r4-core.Observation/schema
           hl7-fhir-r4-core.DiagnosticReport/schema
           hl7-fhir-r4-core.FamilyMemberHistory/schema}},
        :zen/desc "Record of a specific investigation"}}},
     :require #{:code},
     :zen/desc
     "One or more sets of investigations (signs, symptoms, etc.)"}},
   :protocol
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.uri/schema},
     :zen/desc "Clinical Protocol followed"}},
   :assessor
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc "The clinician performing the assessment"},
   :supportingInfo
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference {:refers #{}},
     :zen/desc "Information supporting the clinical impression"}},
   :encounter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "Encounter created as part of"},
   :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :problem
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Condition/schema
        hl7-fhir-r4-core.AllergyIntolerance/schema}},
     :zen/desc "Relevant impressions of patient state"}},
   :statusReason
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "Reason for current status"},
   :_description {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Comments made about the ClinicalImpression"}},
   :summary
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Summary of the assessment"},
   :_protocol
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :prognosisCodeableConcept
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Estimate of likely outcome"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.clinicalimpression-status/value-set,
     :strength :required},
    :zen/desc "in-progress | completed | entered-in-error"},
   :effective
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:dateTime :Period}},
    :type zen/map,
    :keys
    {:dateTime {:confirms #{hl7-fhir-r4-core.dateTime/schema}},
     :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :Period {:confirms #{hl7-fhir-r4-core.Period/schema}}},
    :fhir/flags #{:SU},
    :zen/desc "Time of assessment"},
   :previous
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.ClinicalImpression/schema}},
    :zen/desc "Reference to last assessment"},
   :code
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Kind of assessment performed"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Business identifier"}},
   :_summary {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :finding
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:itemCodeableConcept
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "What was found"},
      :itemReference
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.Media/schema
          hl7-fhir-r4-core.Observation/schema
          hl7-fhir-r4-core.Condition/schema}},
       :zen/desc "What was found"},
      :basis
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Which investigations support finding"},
      :_basis {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :zen/desc "Possible or likely findings and diagnoses"}},
   :prognosisReference
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.RiskAssessment/schema}},
     :zen/desc "RiskAssessment expressing likely outcome"}},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Patient or group assessed"}},
  :zen.fhir/type "ClinicalImpression"}}