{ns hl7-fhir-r4-core.Invoice,
 import
 #{hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.decimal
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.markdown
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Money
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.ChargeItem
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.value-set.invoice-priceComponentType
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.positiveInt
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Account
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization
   hl7-fhir-r4-core.value-set.invoice-status},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Invoice",
  :require #{:status},
  :type zen/map,
  :zen/desc
  "Invoice containing collected ChargeItems from an Account with calculated individual and total price for Billing purpose.",
  :keys
  {:date
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "Invoice date / posting date"},
   :totalNet
   {:confirms #{hl7-fhir-r4-core.Money/schema},
    :fhir/flags #{:SU},
    :zen/desc "Net total of this Invoice"},
   :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :recipient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema
       hl7-fhir-r4-core.Organization/schema
       hl7-fhir-r4-core.RelatedPerson/schema}},
    :zen/desc "Recipient of this invoice"},
   :totalPriceComponent
   {:type zen/vector,
    :every {:zen/desc "Components of Invoice total"}},
   :type
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Type of Invoice"},
   :totalGross
   {:confirms #{hl7-fhir-r4-core.Money/schema},
    :fhir/flags #{:SU},
    :zen/desc "Gross total of this Invoice"},
   :participant
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:role
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Type of involvement in creation of this Invoice"},
      :actor
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.Patient/schema
          hl7-fhir-r4-core.PractitionerRole/schema
          hl7-fhir-r4-core.Organization/schema
          hl7-fhir-r4-core.Device/schema
          hl7-fhir-r4-core.Practitioner/schema
          hl7-fhir-r4-core.RelatedPerson/schema}},
       :zen/desc "Individual who was involved"}},
     :require #{:actor},
     :zen/desc "Participant in creation of this Invoice"}},
   :_cancelledReason {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_paymentTerms {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Comments made about the invoice"}},
   :account
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Account/schema}},
    :zen/desc "Account that is being balanced"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.invoice-status/value-set,
     :strength :required},
    :zen/desc
    "draft | issued | balanced | cancelled | entered-in-error"},
   :lineItem
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:sequence
      {:confirms #{hl7-fhir-r4-core.positiveInt/schema},
       :zen/desc "Sequence number of line item"},
      :_sequence {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :chargeItem
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:CodeableConcept :Reference}},
       :type zen/map,
       :keys
       {:Reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference
         {:refers #{hl7-fhir-r4-core.ChargeItem/schema}}},
        :CodeableConcept
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}}},
       :zen/desc
       "Reference to ChargeItem containing details of this line item or an inline billing code"},
      :priceComponent
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:type
         {:confirms #{hl7-fhir-r4-core.code/schema},
          :zen.fhir/value-set
          {:symbol
           hl7-fhir-r4-core.value-set.invoice-priceComponentType/value-set,
           :strength :required},
          :zen/desc
          "base | surcharge | deduction | discount | tax | informational"},
         :_type {:confirms #{hl7-fhir-r4-core.Element/schema}},
         :code
         {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
          :zen/desc "Code identifying the specific component"},
         :factor
         {:confirms #{hl7-fhir-r4-core.decimal/schema},
          :zen/desc "Factor used for calculating this component"},
         :_factor {:confirms #{hl7-fhir-r4-core.Element/schema}},
         :amount
         {:confirms #{hl7-fhir-r4-core.Money/schema},
          :zen/desc "Monetary amount associated with this component"}},
        :require #{:type},
        :zen/desc "Components of total line item price"}}},
     :require #{:chargeItem},
     :zen/desc "Line items of this Invoice"}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Business Identifier for item"}},
   :issuer
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Issuing Organization of Invoice"},
   :cancelledReason
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Reason for cancellation of this Invoice"},
   :paymentTerms
   {:confirms #{hl7-fhir-r4-core.markdown/schema},
    :zen/desc "Payment details"},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Recipient(s) of goods and services"}},
  :zen.fhir/type "Invoice"}}