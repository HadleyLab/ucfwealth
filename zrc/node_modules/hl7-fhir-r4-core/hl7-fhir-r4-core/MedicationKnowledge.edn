{ns hl7-fhir-r4-core.MedicationKnowledge,
 import
 #{hl7-fhir-r4-core.Duration
   hl7-fhir-r4-core.Substance
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.SimpleQuantity
   hl7-fhir-r4-core.Ratio
   hl7-fhir-r4-core.markdown
   hl7-fhir-r4-core.code
   hl7-fhir-r4-core.DetectedIssue
   zen.fhir
   hl7-fhir-r4-core.ObservationDefinition
   hl7-fhir-r4-core.Money
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Dosage
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.value-set.medicationknowledge-status
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.base64Binary
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Media
   hl7-fhir-r4-core.DocumentReference
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Medication
   hl7-fhir-r4-core.Organization
   hl7-fhir-r4-core.MedicationKnowledge},
 schema
 {:zen/tags #{zen/schema zen.fhir/base-schema},
  :zen/desc
  "Information about a medication that is used to support knowledge.",
  :zen.fhir/type "MedicationKnowledge",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/MedicationKnowledge",
  :zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :type zen/map,
  :keys
  {:preparationInstruction
   {:confirms #{hl7-fhir-r4-core.markdown/schema},
    :zen/desc "The instructions for preparing the medication"},
   :amount
   {:confirms
    #{hl7-fhir-r4-core.Quantity/schema
      hl7-fhir-r4-core.SimpleQuantity/schema},
    :fhir/flags #{:SU},
    :zen/desc "Amount of drug in package"},
   :monograph
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "The category of medication document"},
      :source
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.Media/schema
          hl7-fhir-r4-core.DocumentReference/schema}},
       :zen/desc "Associated documentation about the medication"}},
     :zen/desc "Associated documentation about the medication"}},
   :_preparationInstruction
   {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :regulatory
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:regulatoryAuthority
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.Organization/schema}},
       :zen/desc "Specifies the authority of the regulation"},
      :substitution
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:type
         {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
          :zen/desc "Specifies the type of substitution allowed"},
         :allowed
         {:confirms #{hl7-fhir-r4-core.boolean/schema},
          :zen/desc
          "Specifies if regulation allows for changes in the medication when dispensing"},
         :_allowed {:confirms #{hl7-fhir-r4-core.Element/schema}}},
        :require #{:type :allowed},
        :zen/desc
        "Specifies if changes are allowed when dispensing a medication from a regulatory perspective"}},
      :schedule
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:schedule
         {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
          :zen/desc "Specifies the specific drug schedule"}},
        :require #{:schedule},
        :zen/desc
        "Specifies the schedule of a medication in jurisdiction"}},
      :maxDispense
      {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
       :type zen/map,
       :keys
       {:quantity
        {:confirms
         #{hl7-fhir-r4-core.Quantity/schema
           hl7-fhir-r4-core.SimpleQuantity/schema},
         :zen/desc
         "The maximum number of units of the medication that can be dispensed"},
        :period
        {:confirms #{hl7-fhir-r4-core.Duration/schema},
         :zen/desc
         "The period that applies to the maximum number of units"}},
       :require #{:quantity},
       :zen/desc
       "The maximum number of units of the medication that can be dispensed in a period"}},
     :require #{:regulatoryAuthority},
     :zen/desc "Regulatory information about a medication"}},
   :_synonym
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :doseForm
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "powder | tablets | capsule +"},
   :intendedRoute
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "The intended or approved route of administration"}},
   :drugCharacteristic
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc
       "Code specifying the type of characteristic of medication"},
      :value
      {:fhir/polymorphic true,
       :exclusive-keys
       #{#{:base64Binary :string :CodeableConcept :Quantity}},
       :type zen/map,
       :keys
       {:CodeableConcept
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
        :string {:confirms #{hl7-fhir-r4-core.string/schema}},
        :_string {:confirms #{hl7-fhir-r4-core.Element/schema}},
        :Quantity {:confirms #{hl7-fhir-r4-core.Quantity/schema}},
        :base64Binary
        {:confirms #{hl7-fhir-r4-core.base64Binary/schema}},
        :_base64Binary {:confirms #{hl7-fhir-r4-core.Element/schema}}},
       :zen/desc "Description of the characteristic"}},
     :zen/desc "Specifies descriptive properties of the medicine"}},
   :packaging
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:type
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
      :zen/desc
      "A code that defines the specific type of packaging that the medication can be found in"},
     :quantity
     {:confirms
      #{hl7-fhir-r4-core.Quantity/schema
        hl7-fhir-r4-core.SimpleQuantity/schema},
      :zen/desc
      "The number of product units the package would contain if fully loaded"}},
    :zen/desc "Details about packaged medications"},
   :relatedMedicationKnowledge
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Category of medicationKnowledge"},
      :reference
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
        :zen.fhir/reference
        {:refers #{hl7-fhir-r4-core.MedicationKnowledge/schema}},
        :zen/desc
        "Associated documentation about the associated medication knowledge"},
       :minItems 1}},
     :require #{:type :reference},
     :zen/desc "Associated or related medication information"}},
   :medicineClassification
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc
       "The type of category for the medication (for example, therapeutic classification, therapeutic sub-classification)"},
      :classification
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :zen/desc "Specific category assigned to the medication"}}},
     :require #{:type},
     :zen/desc
     "Categorization of the medication within a formulary or classification system"}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :kinetics
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:areaUnderCurve
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Quantity/schema
          hl7-fhir-r4-core.SimpleQuantity/schema},
        :zen/desc
        "The drug concentration measured at certain discrete points in time"}},
      :lethalDose50
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Quantity/schema
          hl7-fhir-r4-core.SimpleQuantity/schema},
        :zen/desc "The median lethal dose of a drug"}},
      :halfLifePeriod
      {:confirms #{hl7-fhir-r4-core.Duration/schema},
       :zen/desc
       "Time required for concentration in the body to decrease by half"}},
     :zen/desc
     "The time course of drug absorption, distribution, metabolism and excretion of a medication from the body"}},
   :associatedMedication
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Medication/schema}},
     :zen/desc
     "A medication resource that is associated with this medication"}},
   :ingredient
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:item
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:CodeableConcept :Reference}},
       :type zen/map,
       :keys
       {:CodeableConcept
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
        :Reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference
         {:refers #{hl7-fhir-r4-core.Substance/schema}}}},
       :zen/desc
       "Medication(s) or substance(s) contained in the medication"},
      :isActive
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :zen/desc "Active ingredient indicator"},
      :_isActive {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :strength
      {:confirms #{hl7-fhir-r4-core.Ratio/schema},
       :zen/desc "Quantity of ingredient present"}},
     :require #{:item},
     :zen/desc "Active or inactive ingredient"}},
   :monitoringProgram
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc
       "Type of program under which the medication is monitored"},
      :name
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Name of the reviewing program"},
      :_name {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :zen/desc "Program under which a medication is reviewed"}},
   :contraindication
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.DetectedIssue/schema}},
     :zen/desc
     "Potential clinical issue with or between medication(s)"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.medicationknowledge-status/value-set,
     :strength :required},
    :zen/desc "active | inactive | entered-in-error"},
   :productType
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Category of the medication or product"}},
   :synonym
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.string/schema},
     :fhir/flags #{:SU},
     :zen/desc "Additional names for a medication"}},
   :code
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Code that identifies this medication"},
   :administrationGuidelines
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:dosage
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:type
         {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
          :zen/desc "Type of dosage"},
         :dosage
         {:type zen/vector,
          :every
          {:confirms #{hl7-fhir-r4-core.Dosage/schema},
           :zen/desc
           "Dosage for the medication for the specific guidelines"},
          :minItems 1}},
        :require #{:dosage :type},
        :zen/desc
        "Dosage for the medication for the specific guidelines"}},
      :indication
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:CodeableConcept :Reference}},
       :type zen/map,
       :keys
       {:CodeableConcept
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
        :Reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference
         {:refers #{hl7-fhir-r4-core.ObservationDefinition/schema}}}},
       :zen/desc
       "Indication for use that apply to the specific administration guidelines"},
      :patientCharacteristics
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:characteristic
         {:fhir/polymorphic true,
          :exclusive-keys #{#{:CodeableConcept :Quantity}},
          :type zen/map,
          :keys
          {:CodeableConcept
           {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
           :Quantity {:confirms #{hl7-fhir-r4-core.Quantity/schema}}},
          :zen/desc
          "Specific characteristic that is relevant to the administration guideline"},
         :value
         {:type zen/vector,
          :every
          {:confirms #{hl7-fhir-r4-core.string/schema},
           :zen/desc "The specific characteristic"}},
         :_value
         {:type zen/vector,
          :every {:confirms #{hl7-fhir-r4-core.Element/schema}}}},
        :require #{:characteristic},
        :zen/desc
        "Characteristics of the patient that are relevant to the administration guidelines"}}},
     :zen/desc "Guidelines for administration of the medication"}},
   :manufacturer
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Manufacturer of the item"},
   :cost
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "The category of the cost information"},
      :source
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "The source or owner for the price information"},
      :_source {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :cost
      {:confirms #{hl7-fhir-r4-core.Money/schema},
       :zen/desc "The price of the medication"}},
     :require #{:type :cost},
     :zen/desc "The pricing of the medication"}}}}}