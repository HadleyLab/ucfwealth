{ns hl7-fhir-r4-core.VisionPrescription,
 import
 #{hl7-fhir-r4-core.integer
   hl7-fhir-r4-core.decimal
   hl7-fhir-r4-core.value-set.fm-status
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.SimpleQuantity
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.value-set.vision-eye-codes
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.value-set.vision-base-codes},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/VisionPrescription",
  :require
  #{:patient
    :created
    :status
    :prescriber
    :dateWritten
    :lensSpecification},
  :type zen/map,
  :zen/desc
  "An authorization for the provision of glasses and/or contact lenses to a patient.",
  :keys
  {:_created {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc "Who prescription is for"},
   :encounter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "Created during encounter / admission / stay"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :created
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "Response creation date"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.fm-status/value-set,
     :strength :required},
    :zen/desc "active | cancelled | draft | entered-in-error"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc "Business Identifier for vision prescription"}},
   :prescriber
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc "Who authorized the vision prescription"},
   :dateWritten
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "When prescription was authorized"},
   :_dateWritten {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :lensSpecification
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:sphere
      {:confirms #{hl7-fhir-r4-core.decimal/schema},
       :zen/desc "Power of the lens"},
      :_eye {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :_color {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :color
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Color required"},
      :eye
      {:confirms #{hl7-fhir-r4-core.code/schema},
       :fhir/flags #{:SU},
       :zen.fhir/value-set
       {:symbol hl7-fhir-r4-core.value-set.vision-eye-codes/value-set,
        :strength :required},
       :zen/desc "right | left"},
      :_add {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :diameter
      {:confirms #{hl7-fhir-r4-core.decimal/schema},
       :zen/desc "Contact lens diameter"},
      :_axis {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :duration
      {:confirms
       #{hl7-fhir-r4-core.Quantity/schema
         hl7-fhir-r4-core.SimpleQuantity/schema},
       :zen/desc "Lens wear duration"},
      :_diameter {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :brand
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Brand required"},
      :_cylinder {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :note
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.Annotation/schema},
        :zen/desc "Notes for coatings"}},
      :power
      {:confirms #{hl7-fhir-r4-core.decimal/schema},
       :zen/desc "Contact lens power"},
      :product
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "Product to be supplied"},
      :cylinder
      {:confirms #{hl7-fhir-r4-core.decimal/schema},
       :zen/desc "Lens power for astigmatism"},
      :_sphere {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :_brand {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :_power {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :prism
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:amount
         {:confirms #{hl7-fhir-r4-core.decimal/schema},
          :zen/desc "Amount of adjustment"},
         :_amount {:confirms #{hl7-fhir-r4-core.Element/schema}},
         :base
         {:confirms #{hl7-fhir-r4-core.code/schema},
          :zen.fhir/value-set
          {:symbol
           hl7-fhir-r4-core.value-set.vision-base-codes/value-set,
           :strength :required},
          :zen/desc "up | down | in | out"},
         :_base {:confirms #{hl7-fhir-r4-core.Element/schema}}},
        :require #{:amount :base},
        :zen/desc "Eye alignment compensation"}},
      :axis
      {:confirms #{hl7-fhir-r4-core.integer/schema},
       :zen/desc
       "Lens meridian which contain no power for astigmatism"},
      :add
      {:confirms #{hl7-fhir-r4-core.decimal/schema},
       :zen/desc "Added power for multifocal levels"},
      :_backCurve {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :backCurve
      {:confirms #{hl7-fhir-r4-core.decimal/schema},
       :zen/desc "Contact lens back curvature"}},
     :require #{:eye :product},
     :fhir/flags #{:SU},
     :zen/desc "Vision lens authorization"},
    :minItems 1}},
  :zen.fhir/type "VisionPrescription"}}