{ns hl7-fhir-r4-core.Account,
 import
 #{hl7-fhir-r4-core.HealthcareService
   hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.value-set.account-status
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Coverage
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.positiveInt
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Account
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Location
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Account",
  :require #{:status},
  :type zen/map,
  :zen/desc
  "A financial tool for tracking value accrued for a particular purpose.  In the healthcare field, used to track charges for a patient, cost centers, etc.",
  :keys
  {:description
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Explanation of purpose/use"},
   :name
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Human-readable label"},
   :servicePeriod
   {:confirms #{hl7-fhir-r4-core.Period/schema},
    :fhir/flags #{:SU},
    :zen/desc "Transaction window"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :coverage
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:coverage
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :fhir/flags #{:SU},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.Coverage/schema}},
       :zen/desc
       "The party(s), such as insurances, that may contribute to the payment of this account"},
      :priority
      {:confirms #{hl7-fhir-r4-core.positiveInt/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The priority of the coverage in the context of this account"},
      :_priority {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:coverage},
     :fhir/flags #{:SU},
     :zen/desc
     "The party(s) that are responsible for covering the payment of this account, and what order should they be applied to the account"}},
   :type
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "E.g. patient, expense, depreciation"},
   :_description {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :guarantor
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:party
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.Patient/schema
          hl7-fhir-r4-core.Organization/schema
          hl7-fhir-r4-core.RelatedPerson/schema}},
       :zen/desc "Responsible entity"},
      :onHold
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :zen/desc "Credit or other hold applied"},
      :_onHold {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :period
      {:confirms #{hl7-fhir-r4-core.Period/schema},
       :zen/desc "Guarantee account during"}},
     :require #{:party},
     :zen/desc
     "The parties ultimately responsible for balancing the Account"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.account-status/value-set,
     :strength :required},
    :zen/desc
    "active | inactive | entered-in-error | on-hold | unknown"},
   :_name {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Account number"}},
   :partOf
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Account/schema}},
    :zen/desc "Reference to a parent Account"},
   :subject
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Patient/schema
        hl7-fhir-r4-core.PractitionerRole/schema
        hl7-fhir-r4-core.HealthcareService/schema
        hl7-fhir-r4-core.Organization/schema
        hl7-fhir-r4-core.Device/schema
        hl7-fhir-r4-core.Location/schema
        hl7-fhir-r4-core.Practitioner/schema}},
     :zen/desc "The entity that caused the expenses"}},
   :owner
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Entity managing the Account"}},
  :zen.fhir/type "Account"}}