{ns hl7-fhir-r4-core.MedicinalProductIngredient,
 import
 #{hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.Ratio
   zen.fhir
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/MedicinalProductIngredient",
  :require #{:role},
  :type zen/map,
  :zen/desc
  "An ingredient of a manufactured item or pharmaceutical product.",
  :keys
  {:identifier
   {:confirms #{hl7-fhir-r4-core.Identifier/schema},
    :fhir/flags #{:SU},
    :zen/desc "Identifier for the ingredient"},
   :role
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Ingredient role e.g. Active ingredient, excipient"},
   :allergenicIndicator
   {:confirms #{hl7-fhir-r4-core.boolean/schema},
    :fhir/flags #{:SU},
    :zen/desc "If the ingredient is a known or suspected allergen"},
   :_allergenicIndicator
   {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :manufacturer
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Organization/schema}},
     :zen/desc "Manufacturer of this Ingredient"}},
   :specifiedSubstance
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:code
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "The specified substance"},
      :group
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The group of specified substance, e.g. group 1 to 4"},
      :confidentiality
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "Confidentiality level of the specified substance as the ingredient"},
      :strength
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:presentation
         {:confirms #{hl7-fhir-r4-core.Ratio/schema},
          :fhir/flags #{:SU},
          :zen/desc
          "The quantity of substance in the unit of presentation, or in the volume (or mass) of the single pharmaceutical product or manufactured item"},
         :presentationLowLimit
         {:confirms #{hl7-fhir-r4-core.Ratio/schema},
          :fhir/flags #{:SU :?!},
          :zen/desc
          "A lower limit for the quantity of substance in the unit of presentation. For use when there is a range of strengths, this is the lower limit, with the presentation attribute becoming the upper limit"},
         :concentration
         {:confirms #{hl7-fhir-r4-core.Ratio/schema},
          :fhir/flags #{:SU},
          :zen/desc "The strength per unitary volume (or mass)"},
         :concentrationLowLimit
         {:confirms #{hl7-fhir-r4-core.Ratio/schema},
          :fhir/flags #{:SU :?!},
          :zen/desc
          "A lower limit for the strength per unitary volume (or mass), for when there is a range. The concentration attribute then becomes the upper limit"},
         :measurementPoint
         {:confirms #{hl7-fhir-r4-core.string/schema},
          :fhir/flags #{:SU},
          :zen/desc
          "For when strength is measured at a particular point or distance"},
         :_measurementPoint
         {:confirms #{hl7-fhir-r4-core.Element/schema}},
         :country
         {:type zen/vector,
          :every
          {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
           :fhir/flags #{:SU},
           :zen/desc
           "The country or countries for which the strength range applies"}},
         :referenceStrength
         {:type zen/vector,
          :every
          {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
           :type zen/map,
           :keys
           {:substance
            {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
             :fhir/flags #{:SU},
             :zen/desc "Relevant reference substance"},
            :strength
            {:confirms #{hl7-fhir-r4-core.Ratio/schema},
             :fhir/flags #{:SU},
             :zen/desc
             "Strength expressed in terms of a reference substance"},
            :strengthLowLimit
            {:confirms #{hl7-fhir-r4-core.Ratio/schema},
             :fhir/flags #{:SU},
             :zen/desc
             "Strength expressed in terms of a reference substance"},
            :measurementPoint
            {:confirms #{hl7-fhir-r4-core.string/schema},
             :fhir/flags #{:SU},
             :zen/desc
             "For when strength is measured at a particular point or distance"},
            :_measurementPoint
            {:confirms #{hl7-fhir-r4-core.Element/schema}},
            :country
            {:type zen/vector,
             :every
             {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
              :fhir/flags #{:SU},
              :zen/desc
              "The country or countries for which the strength range applies"}}},
           :require #{:strength},
           :fhir/flags #{:SU},
           :zen/desc
           "Strength expressed in terms of a reference substance"}}},
        :require #{:presentation},
        :fhir/flags #{:SU},
        :zen/desc
        "Quantity of the substance or specified substance present in the manufactured item or pharmaceutical product"}}},
     :require #{:group :code},
     :fhir/flags #{:SU},
     :zen/desc
     "A specified substance that comprises this ingredient"}},
   :substance
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:code
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
      :fhir/flags #{:SU},
      :zen/desc "The ingredient substance"},
     :strength
     {:type zen/vector,
      :every
      {:fhir/flags #{:SU},
       :zen/desc
       "Quantity of the substance or specified substance present in the manufactured item or pharmaceutical product"}}},
    :require #{:code},
    :fhir/flags #{:SU},
    :zen/desc "The ingredient substance"}},
  :zen.fhir/type "MedicinalProductIngredient"}}