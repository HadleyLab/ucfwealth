{ns hl7-fhir-r4-core.synthesis,
 import
 #{hl7-fhir-r4-core.markdown
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.value-set.publication-status
   hl7-fhir-r4-core.EvidenceVariable
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.Evidence
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.date
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.RelatedArtifact
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.ContactDetail
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.Identifier},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms #{hl7-fhir-r4-core.Evidence/schema zen.fhir/Resource},
  :zen/tags #{zen.fhir/profile-schema zen/schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/synthesis",
  :require #{:outcome :status :exposureBackground :exposureVariant},
  :type zen/map,
  :zen/desc "Explanation of what this profile contains/is for.",
  :keys
  {:description
   {:confirms #{hl7-fhir-r4-core.markdown/schema},
    :zen/desc "Natural language description of the evidence"},
   :date
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "Date last changed"},
   :publisher
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Name of the publisher (organization or individual)"},
   :approvalDate
   {:confirms #{hl7-fhir-r4-core.date/schema},
    :zen/desc "When the evidence was approved by publisher"},
   :_publisher {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :copyright
   {:confirms #{hl7-fhir-r4-core.markdown/schema},
    :zen/desc "Use and/or publishing restrictions"},
   :_approvalDate {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :outcome
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.EvidenceVariable/schema}},
     :zen/desc "What outcome?"},
    :minItems 1},
   :title
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Name for this evidence (human friendly)"},
   :_description {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Used for footnotes or explanatory notes"}},
   :author
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactDetail/schema},
     :zen/desc "Who authored the content"}},
   :_lastReviewDate {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.publication-status/value-set,
     :strength :required},
    :zen/desc "draft | active | retired | unknown"},
   :subtitle
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Subordinate title of the Evidence"},
   :url
   {:confirms #{hl7-fhir-r4-core.uri/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "Canonical identifier for this evidence, represented as a URI (globally unique)"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Additional identifier for the evidence"}},
   :lastReviewDate
   {:confirms #{hl7-fhir-r4-core.date/schema},
    :zen/desc "When the evidence was last reviewed"},
   :editor
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactDetail/schema},
     :zen/desc "Who edited the content"}},
   :_subtitle {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_copyright {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_shortTitle {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_title {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :reviewer
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactDetail/schema},
     :zen/desc "Who reviewed the content"}},
   :shortTitle
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Title for use in informal contexts"},
   :version
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Business version of the evidence"},
   :_version {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :relatedArtifact
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.RelatedArtifact/schema},
     :zen/desc "Additional documentation, citations, etc."}},
   :contact
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactDetail/schema},
     :fhir/flags #{:SU},
     :zen/desc "Contact details for the publisher"}},
   :exposureBackground
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.EvidenceVariable/schema}},
    :zen/desc "What population?"},
   :_url {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :exposureVariant
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.EvidenceVariable/schema}},
     :zen/desc "What exposure?"},
    :minItems 1,
    :maxItems 2}},
  :zen.fhir/type "Evidence"}}