{ns hl7-fhir-r4-core.DocumentReference,
 import
 #{hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.value-set.document-relationship-type
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.value-set.c80-doc-typecodes
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.instant
   hl7-fhir-r4-core.EpisodeOfCare
   hl7-fhir-r4-core.Coding
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.value-set.document-reference-status
   hl7-fhir-r4-core.value-set.composition-status
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Attachment
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.DocumentReference
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization
   hl7-fhir-r4-core.value-set.formatcodes},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/DocumentReference",
  :require #{:content :status},
  :type zen/map,
  :zen/desc
  "A reference to a document of any kind for any purpose. Provides metadata about the document so that the document can be discovered and managed. The scope of a document is any seralized object with a mime-type, so includes formal patient centric documents (CDA), cliical notes, scanned paper, and non-patient specific documents like policy text.",
  :keys
  {:description
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Human-readable description"},
   :category
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Categorization of document"}},
   :date
   {:confirms #{hl7-fhir-r4-core.instant/schema},
    :fhir/flags #{:SU},
    :zen/desc "When this document reference was created"},
   :docStatus
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.composition-status/value-set,
     :strength :required},
    :zen/desc "preliminary | final | amended | entered-in-error"},
   :content
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:attachment
      {:confirms #{hl7-fhir-r4-core.Attachment/schema},
       :fhir/flags #{:SU},
       :zen/desc "Where to access the document"},
      :format
      {:confirms #{hl7-fhir-r4-core.Coding/schema},
       :fhir/flags #{:SU},
       :zen.fhir/value-set
       {:symbol hl7-fhir-r4-core.value-set.formatcodes/value-set,
        :strength :preferred},
       :zen/desc "Format/content rules for the document"}},
     :require #{:attachment},
     :fhir/flags #{:SU},
     :zen/desc "Document referenced"},
    :minItems 1},
   :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :type
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.c80-doc-typecodes/value-set,
     :strength :preferred},
    :zen/desc "Kind of document (LOINC if possible)"},
   :_description {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :author
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Patient/schema
        hl7-fhir-r4-core.PractitionerRole/schema
        hl7-fhir-r4-core.Organization/schema
        hl7-fhir-r4-core.Device/schema
        hl7-fhir-r4-core.Practitioner/schema
        hl7-fhir-r4-core.RelatedPerson/schema}},
     :zen/desc "Who and/or what authored the document"}},
   :masterIdentifier
   {:confirms #{hl7-fhir-r4-core.Identifier/schema},
    :fhir/flags #{:SU},
    :zen/desc "Master Version Specific Identifier"},
   :custodian
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Organization which maintains the document"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.document-reference-status/value-set,
     :strength :required},
    :zen/desc "current | superseded | entered-in-error"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Other identifiers for the document"}},
   :relatesTo
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:code
      {:confirms #{hl7-fhir-r4-core.code/schema},
       :fhir/flags #{:SU},
       :zen.fhir/value-set
       {:symbol
        hl7-fhir-r4-core.value-set.document-relationship-type/value-set,
        :strength :required},
       :zen/desc "replaces | transforms | signs | appends"},
      :_code {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :target
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :fhir/flags #{:SU},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.DocumentReference/schema}},
       :zen/desc "Target of the relationship"}},
     :require #{:code :target},
     :fhir/flags #{:SU},
     :zen/desc "Relationships to other documents"}},
   :context
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:encounter
     {:type zen/vector,
      :every
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.EpisodeOfCare/schema
          hl7-fhir-r4-core.Encounter/schema}},
       :zen/desc "Context of the document  content"}},
     :event
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Main clinical acts documented"}},
     :period
     {:confirms #{hl7-fhir-r4-core.Period/schema},
      :fhir/flags #{:SU},
      :zen/desc "Time of service that is being documented"},
     :facilityType
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
      :zen/desc "Kind of facility where patient was seen"},
     :practiceSetting
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
      :zen/desc
      "Additional details about where the content was created (e.g. clinical specialty)"},
     :sourcePatientInfo
     {:confirms
      #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
      :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
      :zen/desc "Patient demographics from source"},
     :related
     {:type zen/vector,
      :every
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference {:refers #{}},
       :zen/desc "Related identifiers or resources"}}},
    :fhir/flags #{:SU},
    :zen/desc "Clinical context of document"},
   :securityLabel
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Document security-tags"}},
   :_docStatus {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema
       hl7-fhir-r4-core.Device/schema
       hl7-fhir-r4-core.Practitioner/schema
       hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Who/what is the subject of the document"},
   :authenticator
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Organization/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc "Who/what authenticated the document"}},
  :zen.fhir/type "DocumentReference"}}