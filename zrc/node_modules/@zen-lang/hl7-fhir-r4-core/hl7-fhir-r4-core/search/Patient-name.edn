{ns hl7-fhir-r4-core.search.Patient-name,
 import #{zen.fhir},
 search
 {:zen/tags #{zen.fhir/search},
  :fhir/id "Patient-name",
  :fhir/url "http://hl7.org/fhir/SearchParameter/Patient-name",
  :fhir/type "string",
  :name "name",
  :expr
  {:Patient
   {:knife [["name"]],
    :jsonpath ["$.\"name\"[*]"],
    :data-types #{{:type "HumanName", :polymorphic? false}},
    :template :string,
    :sql
    {:where
     [:or
      [:ilike
       [:pg/cast
        [:pg/jsonb-path-query-array
         [:pg/sql "{{table}}.resource"]
         [:pg/cast
          "($.\"name\"[*]).** ? (@.type() == \"string\")"
          :jsonpath]]
        :text]
       [:pg/sql "{{param}}"]]],
     :parameter-format "%\"?%"}}}}}