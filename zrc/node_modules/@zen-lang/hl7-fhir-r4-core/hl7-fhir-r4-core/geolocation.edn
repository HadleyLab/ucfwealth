{ns hl7-fhir-r4-core.geolocation,
 import #{hl7-fhir-r4-core.decimal zen.fhir},
 schema
 {:zen.fhir/version "0.5.18",
  :zen/tags #{zen/schema zen.fhir/structure-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/geolocation",
  :require #{:longitude :latitude},
  :type zen/map,
  :zen/desc
  "The absolute geographic location of the Location, expressed using the WGS84 datum (This is the same co-ordinate system used in KML).",
  :keys
  {:latitude
   {:confirms #{hl7-fhir-r4-core.decimal/schema},
    :zen/desc "Latitude with WGS84 datum"},
   :longitude
   {:confirms #{hl7-fhir-r4-core.decimal/schema},
    :zen/desc "Longitude with WGS84 datum"}}}}