{ns hl7-fhir-r4-core.ResearchStudy,
 import
 #{hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.markdown
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.ResearchStudy
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.value-set.research-study-status
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.value-set.research-study-objective-type
   hl7-fhir-r4-core.RelatedArtifact
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.ContactDetail
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.PlanDefinition
   hl7-fhir-r4-core.Location
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/ResearchStudy",
  :require #{:status},
  :type zen/map,
  :zen/desc
  "A process where a researcher or organization plans and then executes a series of steps intended to increase the field of healthcare-related knowledge.  This includes studies of safety, efficacy, comparative effectiveness and other information about medications, devices, therapies and other interventional and investigative techniques.  A ResearchStudy involves the gathering of information about human or animal subjects.",
  :keys
  {:description
   {:confirms #{hl7-fhir-r4-core.markdown/schema},
    :zen/desc "What this is study doing"},
   :category
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Classifications for the study"}},
   :enrollment
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Group/schema}},
     :zen/desc "Inclusion & exclusion criteria"}},
   :arm
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:name
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Label for study arm"},
      :_name {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Categorization of study arm"},
      :description
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Short explanation of study path"},
      :_description {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:name},
     :zen/desc "Defined path through the study for a subject"}},
   :site
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Location/schema}},
     :zen/desc "Facility where study activities are conducted"}},
   :protocol
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.PlanDefinition/schema}},
     :zen/desc "Steps followed in executing study"}},
   :principalInvestigator
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc "Researcher who oversees multiple aspects of the study"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :phase
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "n-a | early-phase-1 | phase-1 | phase-1-phase-2 | phase-2 | phase-2-phase-3 | phase-3 | phase-4"},
   :reasonStopped
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "accrual-goal-met | closed-due-to-toxicity | closed-due-to-lack-of-study-progress | temporarily-closed-per-study-design"},
   :title
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Name for this study"},
   :_description {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Comments made about the study"}},
   :keyword
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Used to search for the study"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.research-study-status/value-set,
     :strength :required},
    :zen/desc
    "active | administratively-completed | approved | closed-to-accrual | closed-to-accrual-and-intervention | completed | disapproved | in-review | temporarily-closed-to-accrual | temporarily-closed-to-accrual-and-intervention | withdrawn"},
   :condition
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Condition being studied"}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Business Identifier for study"}},
   :primaryPurposeType
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "treatment | prevention | diagnostic | supportive-care | screening | health-services-research | basic-science | device-feasibility"},
   :focus
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Drugs, devices, etc. under study"}},
   :_title {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :objective
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:name
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Label for the objective"},
      :_name {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen.fhir/value-set
       {:symbol
        hl7-fhir-r4-core.value-set.research-study-objective-type/value-set,
        :strength :preferred},
       :zen/desc "primary | secondary | exploratory"}},
     :zen/desc "A goal for the study"}},
   :period
   {:confirms #{hl7-fhir-r4-core.Period/schema},
    :fhir/flags #{:SU},
    :zen/desc "When the study began and ended"},
   :partOf
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.ResearchStudy/schema}},
     :zen/desc "Part of larger study"}},
   :relatedArtifact
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.RelatedArtifact/schema},
     :zen/desc "References and dependencies"}},
   :location
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Geographic region(s) for study"}},
   :contact
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactDetail/schema},
     :fhir/flags #{:SU},
     :zen/desc "Contact details for the study"}},
   :sponsor
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc
    "Organization that initiates and is legally responsible for the study"}},
  :zen.fhir/type "ResearchStudy"}}