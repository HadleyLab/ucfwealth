{ns hl7-fhir-r4-core.DeviceUseStatement,
 import
 #{hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.Observation
   hl7-fhir-r4-core.Condition
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.value-set.device-statement-status
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.Timing
   hl7-fhir-r4-core.DiagnosticReport
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.QuestionnaireResponse
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.ServiceRequest
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Media
   hl7-fhir-r4-core.Procedure
   hl7-fhir-r4-core.DocumentReference
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Claim},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/DeviceUseStatement",
  :require #{:status :device :subject},
  :type zen/map,
  :zen/desc
  "A record of a device being used by a patient where the record is the result of a report from the patient or another clinician.",
  :keys
  {:_recordedOn {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :derivedFrom
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.QuestionnaireResponse/schema
        hl7-fhir-r4-core.Observation/schema
        hl7-fhir-r4-core.DocumentReference/schema
        hl7-fhir-r4-core.ServiceRequest/schema
        hl7-fhir-r4-core.Procedure/schema
        hl7-fhir-r4-core.Claim/schema}},
     :zen/desc "Supporting information"}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :reasonCode
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Why device was used"}},
   :source
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema
       hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema
       hl7-fhir-r4-core.RelatedPerson/schema}},
    :zen/desc "Who made the statement"},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Addition details (comments, instructions)"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.device-statement-status/value-set,
     :strength :required},
    :zen/desc "active | completed | entered-in-error +"},
   :timing
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:dateTime :Period :Timing}},
    :type zen/map,
    :keys
    {:Timing {:confirms #{hl7-fhir-r4-core.Timing/schema}},
     :Period {:confirms #{hl7-fhir-r4-core.Period/schema}},
     :dateTime {:confirms #{hl7-fhir-r4-core.dateTime/schema}},
     :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :fhir/flags #{:SU},
    :zen/desc "How often  the device was used"},
   :recordedOn
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "When statement was recorded"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "External identifier for this record"}},
   :bodySite
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Target body site"},
   :device
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Device/schema}},
    :zen/desc "Reference to device used"},
   :basedOn
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.ServiceRequest/schema}},
     :zen/desc "Fulfills plan, proposal or order"}},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Patient using device"},
   :reasonReference
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Media/schema
        hl7-fhir-r4-core.Observation/schema
        hl7-fhir-r4-core.DocumentReference/schema
        hl7-fhir-r4-core.DiagnosticReport/schema
        hl7-fhir-r4-core.Condition/schema}},
     :zen/desc "Why was DeviceUseStatement performed?"}}},
  :zen.fhir/type "DeviceUseStatement"}}