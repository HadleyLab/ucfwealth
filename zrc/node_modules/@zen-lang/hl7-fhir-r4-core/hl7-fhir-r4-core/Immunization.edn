{ns hl7-fhir-r4-core.Immunization,
 import
 #{hl7-fhir-r4-core.Observation
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.SimpleQuantity
   hl7-fhir-r4-core.Condition
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.DiagnosticReport
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.date
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.positiveInt
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.value-set.immunization-status
   hl7-fhir-r4-core.Location
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Immunization",
  :require #{:patient :vaccineCode :status :occurrence},
  :type zen/map,
  :zen/desc
  "Describes the event of a patient being administered a vaccine or a record of an immunization as reported by a patient, a clinician or another party.",
  :keys
  {:patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc "Who was immunized"},
   :isSubpotent
   {:confirms #{hl7-fhir-r4-core.boolean/schema},
    :fhir/flags #{:SU :?!},
    :zen/desc "Dose potency"},
   :reportOrigin
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "Indicates the source of a secondarily reported record"},
   :protocolApplied
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:series
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Name of vaccine series"},
      :_series {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :authority
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.Organization/schema}},
       :zen/desc
       "Who is responsible for publishing the recommendations"},
      :targetDisease
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :zen/desc "Vaccine preventatable disease being targetted"}},
      :doseNumber
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:string :positiveInt}},
       :type zen/map,
       :keys
       {:positiveInt
        {:confirms #{hl7-fhir-r4-core.positiveInt/schema}},
        :_positiveInt {:confirms #{hl7-fhir-r4-core.Element/schema}},
        :string {:confirms #{hl7-fhir-r4-core.string/schema}},
        :_string {:confirms #{hl7-fhir-r4-core.Element/schema}}},
       :zen/desc "Dose number within series"},
      :seriesDoses
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:string :positiveInt}},
       :type zen/map,
       :keys
       {:positiveInt
        {:confirms #{hl7-fhir-r4-core.positiveInt/schema}},
        :_positiveInt {:confirms #{hl7-fhir-r4-core.Element/schema}},
        :string {:confirms #{hl7-fhir-r4-core.string/schema}},
        :_string {:confirms #{hl7-fhir-r4-core.Element/schema}}},
       :zen/desc "Recommended number of doses for immunity"}},
     :require #{:doseNumber},
     :zen/desc "Protocol followed by the provider"}},
   :site
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "Body site vaccine  was administered"},
   :_isSubpotent {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :encounter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "Encounter immunization was part of"},
   :vaccineCode
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Vaccine product administered"},
   :doseQuantity
   {:confirms
    #{hl7-fhir-r4-core.Quantity/schema
      hl7-fhir-r4-core.SimpleQuantity/schema},
    :zen/desc "Amount of vaccine administered"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :reasonCode
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Why immunization occurred"}},
   :statusReason
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "Reason not done"},
   :route
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "How vaccine entered body"},
   :recorded
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc
    "When the immunization was first captured in the subject's record"},
   :_expirationDate {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :programEligibility
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Patient eligibility for a vaccination program"}},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :fhir/flags #{:SU},
     :zen/desc "Additional immunization notes"}},
   :primarySource
   {:confirms #{hl7-fhir-r4-core.boolean/schema},
    :fhir/flags #{:SU},
    :zen/desc "Indicates context the data was recorded in"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.immunization-status/value-set,
     :strength :required},
    :zen/desc "completed | entered-in-error | not-done"},
   :lotNumber
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :zen/desc "Vaccine lot number"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc "Business identifier"}},
   :manufacturer
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Vaccine manufacturer"},
   :education
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:documentType
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Educational material document identifier"},
      :_documentType {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :reference
      {:confirms #{hl7-fhir-r4-core.uri/schema},
       :zen/desc "Educational material reference pointer"},
      :_reference {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :publicationDate
      {:confirms #{hl7-fhir-r4-core.dateTime/schema},
       :zen/desc "Educational material publication date"},
      :_publicationDate {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :presentationDate
      {:confirms #{hl7-fhir-r4-core.dateTime/schema},
       :zen/desc "Educational material presentation date"},
      :_presentationDate
      {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :zen/desc "Educational material presented to patient"}},
   :_primarySource {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_recorded {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :_lotNumber {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :reaction
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:date
      {:confirms #{hl7-fhir-r4-core.dateTime/schema},
       :zen/desc "When reaction started"},
      :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :detail
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.Observation/schema}},
       :zen/desc "Additional information on reaction"},
      :reported
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :zen/desc "Indicates self-reported reaction"},
      :_reported {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :zen/desc "Details of a reaction that follows immunization"}},
   :location
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Location/schema}},
    :zen/desc "Where immunization occurred"},
   :fundingSource
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "Funding source for the vaccine"},
   :subpotentReason
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Reason for being subpotent"}},
   :occurrence
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:string :dateTime}},
    :type zen/map,
    :keys
    {:dateTime {:confirms #{hl7-fhir-r4-core.dateTime/schema}},
     :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :string {:confirms #{hl7-fhir-r4-core.string/schema}},
     :_string {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :fhir/flags #{:SU},
    :zen/desc "Vaccine administration date"},
   :expirationDate
   {:confirms #{hl7-fhir-r4-core.date/schema},
    :zen/desc "Vaccine expiration date"},
   :performer
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:function
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "What type of performance was done"},
      :actor
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :fhir/flags #{:SU},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.PractitionerRole/schema
          hl7-fhir-r4-core.Organization/schema
          hl7-fhir-r4-core.Practitioner/schema}},
       :zen/desc "Individual or organization who was performing"}},
     :require #{:actor},
     :fhir/flags #{:SU},
     :zen/desc "Who performed event"}},
   :reasonReference
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Observation/schema
        hl7-fhir-r4-core.DiagnosticReport/schema
        hl7-fhir-r4-core.Condition/schema}},
     :zen/desc "Why immunization occurred"}}},
  :zen.fhir/type "Immunization"}}