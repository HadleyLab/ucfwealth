{ns hl7-fhir-r4-core.CareTeam,
 import
 #{hl7-fhir-r4-core.CareTeam
   hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.value-set.care-team-status
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.Condition
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.ContactPoint
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization},
 schema
 {:zen/tags #{zen/schema zen.fhir/base-schema},
  :zen/desc
  "The Care Team includes all the people and organizations who plan to participate in the coordination and delivery of care for a patient.",
  :zen.fhir/type "CareTeam",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/CareTeam",
  :zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :type zen/map,
  :keys
  {:category
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Type of team"}},
   :managingOrganization
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Organization/schema}},
     :zen/desc "Organization responsible for the care team"}},
   :encounter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "Encounter created as part of"},
   :name
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Name of the team, such as crisis assessment team"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :reasonCode
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Why the care team exists"}},
   :participant
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:role
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :fhir/flags #{:SU},
        :zen/desc "Type of involvement"}},
      :member
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :fhir/flags #{:SU},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.CareTeam/schema
          hl7-fhir-r4-core.Patient/schema
          hl7-fhir-r4-core.PractitionerRole/schema
          hl7-fhir-r4-core.Organization/schema
          hl7-fhir-r4-core.Practitioner/schema
          hl7-fhir-r4-core.RelatedPerson/schema}},
       :zen/desc "Who is involved"},
      :onBehalfOf
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :fhir/flags #{:SU},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.Organization/schema}},
       :zen/desc "Organization of the practitioner"},
      :period
      {:confirms #{hl7-fhir-r4-core.Period/schema},
       :zen/desc "Time period of participant"}},
     :zen/desc "Members of the team"}},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Comments made about the CareTeam"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.care-team-status/value-set,
     :strength :required},
    :zen/desc
    "proposed | active | suspended | inactive | entered-in-error"},
   :_name {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "External Ids for this team"}},
   :telecom
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.ContactPoint/schema},
     :zen/desc
     "A contact detail for the care team (that applies to all members)"}},
   :period
   {:confirms #{hl7-fhir-r4-core.Period/schema},
    :fhir/flags #{:SU},
    :zen/desc "Time period team covers"},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Who care team is for"},
   :reasonReference
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Condition/schema}},
     :zen/desc "Why the care team exists"}}}}}