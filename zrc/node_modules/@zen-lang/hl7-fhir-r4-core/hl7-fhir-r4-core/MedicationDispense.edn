{ns hl7-fhir-r4-core.MedicationDispense,
 import
 #{hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.SimpleQuantity
   hl7-fhir-r4-core.code
   hl7-fhir-r4-core.DetectedIssue
   zen.fhir
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.value-set.medicationdispense-status
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.EpisodeOfCare
   hl7-fhir-r4-core.MedicationRequest
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Dosage
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.value-set.medicationdispense-category
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Procedure
   hl7-fhir-r4-core.Provenance
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Location
   hl7-fhir-r4-core.Medication
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/MedicationDispense",
  :require #{:status :medication},
  :type zen/map,
  :zen/desc
  "Indicates that a medication product is to be or has been dispensed for a named person/patient.  This includes a description of the medication product (supply) provided and the instructions for administering the medication.  The medication dispense is the result of a pharmacy system responding to a medication order.",
  :keys
  {:_whenPrepared {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :category
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.medicationdispense-category/value-set,
     :strength :preferred},
    :zen/desc "Type of medication dispense"},
   :whenHandedOver
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc "When product was given out"},
   :whenPrepared
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "When product was packaged and reviewed"},
   :eventHistory
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Provenance/schema}},
     :zen/desc "A list of relevant lifecycle events"}},
   :substitution
   {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
    :type zen/map,
    :keys
    {:wasSubstituted
     {:confirms #{hl7-fhir-r4-core.boolean/schema},
      :zen/desc
      "Whether a substitution was or was not performed on the dispense"},
     :_wasSubstituted {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :type
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
      :zen/desc
      "Code signifying whether a different drug was dispensed from what was prescribed"},
     :reason
     {:type zen/vector,
      :every
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Why was substitution made"}},
     :responsibleParty
     {:type zen/vector,
      :every
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.PractitionerRole/schema
          hl7-fhir-r4-core.Practitioner/schema}},
       :zen/desc "Who is responsible for the substitution"}}},
    :require #{:wasSubstituted},
    :zen/desc "Whether a substitution was performed on the dispense"},
   :detectedIssue
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.DetectedIssue/schema}},
     :zen/desc "Clinical issue with action"}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :type
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "Trial fill, partial fill, emergency fill, etc."},
   :statusReason
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:CodeableConcept :Reference}},
    :type zen/map,
    :keys
    {:CodeableConcept
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
     :Reference
     {:confirms
      #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
      :zen.fhir/reference
      {:refers #{hl7-fhir-r4-core.DetectedIssue/schema}}}},
    :zen/desc "Why a dispense was not performed"},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Information about the dispense"}},
   :supportingInformation
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference {:refers #{}},
     :zen/desc
     "Information that supports the dispensing of the medication"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.medicationdispense-status/value-set,
     :strength :required},
    :zen/desc
    "preparation | in-progress | cancelled | on-hold | completed | entered-in-error | stopped | declined | unknown"},
   :dosageInstruction
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Dosage/schema},
     :zen/desc
     "How the medication is to be used by the patient or administered by the caregiver"}},
   :daysSupply
   {:confirms
    #{hl7-fhir-r4-core.Quantity/schema
      hl7-fhir-r4-core.SimpleQuantity/schema},
    :zen/desc "Amount of medication expressed as a timing amount"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc "External identifier"}},
   :context
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.EpisodeOfCare/schema
       hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "Encounter / Episode associated with event"},
   :quantity
   {:confirms
    #{hl7-fhir-r4-core.Quantity/schema
      hl7-fhir-r4-core.SimpleQuantity/schema},
    :zen/desc "Amount dispensed"},
   :partOf
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.Procedure/schema}},
     :zen/desc "Event that dispense is part of"}},
   :_whenHandedOver {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :location
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Location/schema}},
    :zen/desc "Where the dispense occurred"},
   :authorizingPrescription
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.MedicationRequest/schema}},
     :zen/desc "Medication order that authorizes the dispense"}},
   :receiver
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Patient/schema
        hl7-fhir-r4-core.Practitioner/schema}},
     :zen/desc "Who collected the medication"}},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Who the dispense is for"},
   :destination
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Location/schema}},
    :zen/desc "Where the medication was sent"},
   :performer
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:function
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Who performed the dispense and what they did"},
      :actor
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.Patient/schema
          hl7-fhir-r4-core.PractitionerRole/schema
          hl7-fhir-r4-core.Organization/schema
          hl7-fhir-r4-core.Device/schema
          hl7-fhir-r4-core.Practitioner/schema
          hl7-fhir-r4-core.RelatedPerson/schema}},
       :zen/desc "Individual who was performing"}},
     :require #{:actor},
     :zen/desc "Who performed event"}},
   :medication
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:CodeableConcept :Reference}},
    :type zen/map,
    :keys
    {:CodeableConcept
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
     :Reference
     {:confirms
      #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
      :zen.fhir/reference
      {:refers #{hl7-fhir-r4-core.Medication/schema}}}},
    :fhir/flags #{:SU},
    :zen/desc "What medication was supplied"}},
  :zen.fhir/type "MedicationDispense"}}