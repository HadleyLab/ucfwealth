{ns hl7-fhir-r4-core.MedicinalProduct,
 import
 #{hl7-fhir-r4-core.MedicinalProductPharmaceutical
   hl7-fhir-r4-core.BackboneElement
   zen.fhir
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.ResearchStudy
   hl7-fhir-r4-core.MarketingStatus
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.MedicinalProductIndication
   hl7-fhir-r4-core.Coding
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.MedicinalProductPackaged
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.DocumentReference
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/MedicinalProduct",
  :require #{:name},
  :type zen/map,
  :zen/desc
  "Detailed definition of a medicinal product, typically for uses other than direct patient care (e.g. regulatory use).",
  :keys
  {:additionalMonitoringIndicator
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "Whether the Medicinal Product is subject to additional monitoring for regulatory reasons"},
   :manufacturingBusinessOperation
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:operationType
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "The type of manufacturing operation"},
      :authorisationReferenceNumber
      {:confirms #{hl7-fhir-r4-core.Identifier/schema},
       :fhir/flags #{:SU},
       :zen/desc "Regulatory authorization reference number"},
      :effectiveDate
      {:confirms #{hl7-fhir-r4-core.dateTime/schema},
       :fhir/flags #{:SU},
       :zen/desc "Regulatory authorization date"},
      :_effectiveDate {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :confidentialityIndicator
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "To indicate if this proces is commercially confidential"},
      :manufacturer
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
        :fhir/flags #{:SU},
        :zen.fhir/reference
        {:refers #{hl7-fhir-r4-core.Organization/schema}},
        :zen/desc
        "The manufacturer or establishment associated with the process"}},
      :regulator
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :fhir/flags #{:SU},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.Organization/schema}},
       :zen/desc "A regulator which oversees the operation"}},
     :fhir/flags #{:SU},
     :zen/desc
     "An operation applied to the product, for manufacturing or adminsitrative purpose"}},
   :combinedPharmaceuticalDoseForm
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The dose form for a single part product, or combined form of a multiple part product"},
   :clinicalTrial
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.ResearchStudy/schema}},
     :zen/desc
     "Clinical trials or studies that this product is involved in"}},
   :productClassification
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Allows the product to be classified by various systems"}},
   :name
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:productName
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :fhir/flags #{:SU},
       :zen/desc "The full product name"},
      :_productName {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :namePart
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:part
         {:confirms #{hl7-fhir-r4-core.string/schema},
          :fhir/flags #{:SU},
          :zen/desc "A fragment of a product name"},
         :_part {:confirms #{hl7-fhir-r4-core.Element/schema}},
         :type
         {:confirms #{hl7-fhir-r4-core.Coding/schema},
          :fhir/flags #{:SU},
          :zen/desc
          "Idenifying type for this part of the name (e.g. strength part)"}},
        :require #{:part :type},
        :fhir/flags #{:SU},
        :zen/desc "Coding words or phrases of the name"}},
      :countryLanguage
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:country
         {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
          :fhir/flags #{:SU},
          :zen/desc "Country code for where this name applies"},
         :jurisdiction
         {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
          :fhir/flags #{:SU},
          :zen/desc "Jurisdiction code for where this name applies"},
         :language
         {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
          :fhir/flags #{:SU},
          :zen/desc "Language code for this name"}},
        :require #{:language :country},
        :fhir/flags #{:SU},
        :zen/desc "Country where the name applies"}}},
     :require #{:productName},
     :fhir/flags #{:SU},
     :zen/desc
     "The product's name, including full name and possibly coded parts"},
    :minItems 1},
   :masterFile
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.DocumentReference/schema}},
     :zen/desc
     "A master file for to the medicinal product (e.g. Pharmacovigilance System Master File)"}},
   :pharmaceuticalProduct
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.MedicinalProductPharmaceutical/schema}},
     :zen/desc "Pharmaceutical aspects of product"}},
   :type
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Regulatory type, e.g. Investigational or Authorized"},
   :marketingStatus
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.MarketingStatus/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Marketing status of the medicinal product, in contrast to marketing authorizaton"}},
   :specialMeasures
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.string/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Whether the Medicinal Product is subject to special measures for regulatory reasons"}},
   :specialDesignation
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:identifier
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.Identifier/schema},
        :fhir/flags #{:SU},
        :zen/desc
        "Identifier for the designation, or procedure number"}},
      :type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The type of special designation, e.g. orphan drug, minor use"},
      :intendedUse
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The intended use of the product, e.g. prevention, treatment"},
      :indication
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:CodeableConcept :Reference}},
       :type zen/map,
       :keys
       {:CodeableConcept
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
        :Reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference
         {:refers
          #{hl7-fhir-r4-core.MedicinalProductIndication/schema}}}},
       :fhir/flags #{:SU},
       :zen/desc "Condition for which the medicinal use applies"},
      :status
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "For example granted, pending, expired or withdrawn"},
      :date
      {:confirms #{hl7-fhir-r4-core.dateTime/schema},
       :fhir/flags #{:SU},
       :zen/desc "Date when the designation was granted"},
      :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :species
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc "Animal species for which this applies"}},
     :fhir/flags #{:SU},
     :zen/desc
     "Indicates if the medicinal product has an orphan designation for the treatment of a rare disease"}},
   :packagedMedicinalProduct
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.MedicinalProductPackaged/schema}},
     :zen/desc "Package representation for the product"}},
   :_specialMeasures
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Business identifier for this product. Could be an MPID"}},
   :crossReference
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc
     "Reference to another product, e.g. for linking authorised to investigational product"}},
   :attachedDocument
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.DocumentReference/schema}},
     :zen/desc
     "Supporting documentation, typically for regulatory submission"}},
   :domain
   {:confirms #{hl7-fhir-r4-core.Coding/schema},
    :fhir/flags #{:SU},
    :zen/desc "If this medicine applies to human or veterinary uses"},
   :legalStatusOfSupply
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The legal status of supply of the medicinal product as classified by the regulator"},
   :paediatricUseIndicator
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "If authorised for use in children"},
   :contact
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.PractitionerRole/schema
        hl7-fhir-r4-core.Organization/schema}},
     :zen/desc
     "A product specific contact, person (in a role), or an organization"}}},
  :zen.fhir/type "MedicinalProduct"}}