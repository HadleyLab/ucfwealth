       :fhir/flags #{:SU},
       :zen/desc "A date valued parameter to search on"},
      :_searchParam {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :value
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:dateTime :Period :Duration}},
       :type zen/map,
       :keys
       {:dateTime {:confirms #{hl7-fhir-r4-core.dateTime/schema}},
        :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}},
        :Period {:confirms #{hl7-fhir-r4-core.Period/schema}},
        :Duration {:confirms #{hl7-fhir-r4-core.Duration/schema}}},
       :fhir/flags #{:SU},
       :zen/desc
       "The value of the filter, as a Period, DateTime, or Duration value"}},
     :fhir/flags #{:SU},
     :zen/desc "What dates/date ranges are expected"}},
