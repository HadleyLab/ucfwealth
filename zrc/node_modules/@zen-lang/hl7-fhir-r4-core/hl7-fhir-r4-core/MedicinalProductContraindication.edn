{ns hl7-fhir-r4-core.MedicinalProductContraindication,
 import
 #{hl7-fhir-r4-core.Substance
   hl7-fhir-r4-core.BackboneElement
   zen.fhir
   hl7-fhir-r4-core.MedicinalProductIndication
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.SubstanceSpecification
   hl7-fhir-r4-core.Population
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.MedicinalProduct
   hl7-fhir-r4-core.Medication},
 schema
 {:zen/tags #{zen/schema zen.fhir/base-schema},
  :zen/desc
  "The clinical particulars - indications, contraindications etc. of a medicinal product, including for regulatory purposes.",
  :zen.fhir/type "MedicinalProductContraindication",
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/MedicinalProductContraindication",
  :zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :type zen/map,
  :keys
  {:subject
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Medication/schema
        hl7-fhir-r4-core.MedicinalProduct/schema}},
     :zen/desc "The medication for which this is an indication"}},
   :disease
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The disease, symptom or procedure for the contraindication"},
   :diseaseStatus
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The status of the disease or symptom for the contraindication"},
   :comorbidity
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "A comorbidity (concurrent condition) or coinfection"}},
   :therapeuticIndication
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.MedicinalProductIndication/schema}},
     :zen/desc
     "Information about the use of the medicinal product in relation to other therapies as part of the indication"}},
   :otherTherapy
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:therapyRelationshipType
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :fhir/flags #{:SU},
       :zen/desc
       "The type of relationship between the medicinal product indication or contraindication and another therapy"},
      :medication
      {:fhir/polymorphic true,
       :exclusive-keys #{#{:CodeableConcept :Reference}},
       :type zen/map,
       :keys
       {:CodeableConcept
        {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
        :Reference
        {:confirms
         #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
         :zen.fhir/reference
         {:refers
          #{hl7-fhir-r4-core.Medication/schema
            hl7-fhir-r4-core.SubstanceSpecification/schema
            hl7-fhir-r4-core.Substance/schema
            hl7-fhir-r4-core.MedicinalProduct/schema}}}},
       :fhir/flags #{:SU},
       :zen/desc
       "Reference to a specific medication (active substance, medicinal product or class of products) as part of an indication or contraindication"}},
     :require #{:therapyRelationshipType :medication},
     :fhir/flags #{:SU},
     :zen/desc
     "Information about the use of the medicinal product in relation to other therapies described as part of the indication"}},
   :population
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Population/schema},
     :fhir/flags #{:SU},
     :zen/desc "The population group to which this applies"}}}}}