          :keys
          {:Duration {:confirms #{hl7-fhir-r4-core.Duration/schema}},
           :Range {:confirms #{hl7-fhir-r4-core.Range/schema}}},
          :zen/desc "Time offset for the relationship"}},
        :require #{:relationship :actionId},
        :zen/desc "Relationship to another action"}},
      :_priority {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "create | update | remove | fire-event"},
      :participant
      {:type zen/vector,
       :every
