{ns hl7-fhir-r4-core.DocumentManifest,
 import
 #{hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.value-set.document-reference-status
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/DocumentManifest",
  :require #{:content :status},
  :type zen/map,
  :zen/desc
  "A collection of documents compiled for a purpose together with metadata that applies to the collection.",
  :keys
  {:_created {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :description
   {:confirms #{hl7-fhir-r4-core.string/schema},
    :fhir/flags #{:SU},
    :zen/desc "Human-readable description (title)"},
   :content
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference {:refers #{}},
     :zen/desc "Items in manifest"},
    :minItems 1},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :recipient
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Patient/schema
        hl7-fhir-r4-core.PractitionerRole/schema
        hl7-fhir-r4-core.Organization/schema
        hl7-fhir-r4-core.Practitioner/schema
        hl7-fhir-r4-core.RelatedPerson/schema}},
     :zen/desc
     "Intended to get notified about this set of documents"}},
   :type
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Kind of document set"},
   :created
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc "When this document manifest created"},
   :related
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:identifier
      {:confirms #{hl7-fhir-r4-core.Identifier/schema},
       :zen/desc "Identifiers of things that are related"},
      :ref
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference {:refers #{}},
       :zen/desc "Related Resource"}},
     :zen/desc "Related things"}},
   :source
   {:confirms #{hl7-fhir-r4-core.uri/schema},
    :zen/desc "The source system/application/software"},
   :_description {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :author
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :fhir/flags #{:SU},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Patient/schema
        hl7-fhir-r4-core.PractitionerRole/schema
        hl7-fhir-r4-core.Organization/schema
        hl7-fhir-r4-core.Device/schema
        hl7-fhir-r4-core.Practitioner/schema
        hl7-fhir-r4-core.RelatedPerson/schema}},
     :zen/desc "Who and/or what authored the DocumentManifest"}},
   :masterIdentifier
   {:confirms #{hl7-fhir-r4-core.Identifier/schema},
    :fhir/flags #{:SU},
    :zen/desc "Unique Identifier for the set of documents"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.document-reference-status/value-set,
     :strength :required},
    :zen/desc "current | superseded | entered-in-error"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Other identifiers for the manifest"}},
   :_source {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema
       hl7-fhir-r4-core.Device/schema
       hl7-fhir-r4-core.Practitioner/schema
       hl7-fhir-r4-core.Group/schema}},
    :zen/desc "The subject of the set of documents"}},
  :zen.fhir/type "DocumentManifest"}}