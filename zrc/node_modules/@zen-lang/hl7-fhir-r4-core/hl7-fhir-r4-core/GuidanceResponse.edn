{ns hl7-fhir-r4-core.GuidanceResponse,
 import
 #{hl7-fhir-r4-core.OperationOutcome
   hl7-fhir-r4-core.Device
   hl7-fhir-r4-core.Observation
   hl7-fhir-r4-core.value-set.guidance-response-status
   hl7-fhir-r4-core.Condition
   hl7-fhir-r4-core.canonical
   hl7-fhir-r4-core.CarePlan
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.DiagnosticReport
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.uri
   hl7-fhir-r4-core.RequestGroup
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.DataRequirement
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.DocumentReference
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Parameters},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/GuidanceResponse",
  :require #{:module :status},
  :type zen/map,
  :zen/desc
  "A guidance response is the formal response to a guidance request, including any output parameters returned by the evaluation, as well as the description of any proposed actions to be taken.",
  :keys
  {:dataRequirement
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.DataRequirement/schema},
     :zen/desc "Additional required data"}},
   :encounter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "Encounter during which the response was returned"},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :reasonCode
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "Why guidance is needed"}},
   :outputParameters
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Parameters/schema}},
    :zen/desc "The output parameters of the evaluation, if any"},
   :evaluationMessage
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers #{hl7-fhir-r4-core.OperationOutcome/schema}},
     :zen/desc
     "Messages resulting from the evaluation of the artifact or artifacts"}},
   :requestIdentifier
   {:confirms #{hl7-fhir-r4-core.Identifier/schema},
    :fhir/flags #{:SU},
    :zen/desc
    "The identifier of the request associated with this response, if any"},
   :module
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:canonical :uri :CodeableConcept}},
    :type zen/map,
    :keys
    {:uri {:confirms #{hl7-fhir-r4-core.uri/schema}},
     :_uri {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :canonical {:confirms #{hl7-fhir-r4-core.canonical/schema}},
     :_canonical {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :CodeableConcept
     {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}}},
    :fhir/flags #{:SU},
    :zen/desc "What guidance was requested"},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Additional notes about the response"}},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol
     hl7-fhir-r4-core.value-set.guidance-response-status/value-set,
     :strength :required},
    :zen/desc
    "success | data-requested | data-required | in-progress | failure | entered-in-error"},
   :result
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.RequestGroup/schema
       hl7-fhir-r4-core.CarePlan/schema}},
    :zen/desc "Proposed actions, if any"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "Business identifier"}},
   :occurrenceDateTime
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :zen/desc "When the guidance response was processed"},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Patient the request was performed for"},
   :performer
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Device/schema}},
    :zen/desc "Device returning the guidance"},
   :reasonReference
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
     :zen.fhir/reference
     {:refers
      #{hl7-fhir-r4-core.Observation/schema
        hl7-fhir-r4-core.DocumentReference/schema
        hl7-fhir-r4-core.DiagnosticReport/schema
        hl7-fhir-r4-core.Condition/schema}},
     :zen/desc "Why guidance is needed"}},
   :_occurrenceDateTime
   {:confirms #{hl7-fhir-r4-core.Element/schema}}},
  :zen.fhir/type "GuidanceResponse"}}