{ns hl7-fhir-r4-core.Condition,
 import
 #{hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.Observation
   hl7-fhir-r4-core.ClinicalImpression
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.value-set.condition-severity
   zen.fhir
   hl7-fhir-r4-core.value-set.condition-ver-status
   hl7-fhir-r4-core.Annotation
   hl7-fhir-r4-core.Group
   hl7-fhir-r4-core.RelatedPerson
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.DiagnosticReport
   hl7-fhir-r4-core.Range
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.value-set.condition-clinical
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.Encounter
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Age},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/Condition",
  :require #{:subject},
  :type zen/map,
  :zen/desc
  "A clinical condition, problem, diagnosis, or other event, situation, issue, or clinical concept that has risen to a level of concern.",
  :keys
  {:onset
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:string :dateTime :Range :Period :Age}},
    :type zen/map,
    :keys
    {:dateTime {:confirms #{hl7-fhir-r4-core.dateTime/schema}},
     :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :Age {:confirms #{hl7-fhir-r4-core.Age/schema}},
     :Period {:confirms #{hl7-fhir-r4-core.Period/schema}},
     :Range {:confirms #{hl7-fhir-r4-core.Range/schema}},
     :string {:confirms #{hl7-fhir-r4-core.string/schema}},
     :_string {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :fhir/flags #{:SU},
    :zen/desc "Estimated or actual date,  date-time, or age"},
   :category
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :zen/desc "problem-list-item | encounter-diagnosis"}},
   :clinicalStatus
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.condition-clinical/value-set,
     :strength :required},
    :zen/desc
    "active | recurrence | relapse | inactive | remission | resolved"},
   :stage
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:summary
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Simple summary (disease specific)"},
      :assessment
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
        :zen.fhir/reference
        {:refers
         #{hl7-fhir-r4-core.Observation/schema
           hl7-fhir-r4-core.ClinicalImpression/schema
           hl7-fhir-r4-core.DiagnosticReport/schema}},
        :zen/desc "Formal record of assessment"}},
      :type
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Kind of staging"}},
     :zen/desc "Stage/grade, usually assessed formally"}},
   :encounter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Encounter/schema}},
    :zen/desc "Encounter created as part of"},
   :evidence
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:code
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :fhir/flags #{:SU},
        :zen/desc "Manifestation/symptom"}},
      :detail
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
        :fhir/flags #{:SU},
        :zen.fhir/reference {:refers #{}},
        :zen/desc "Supporting information found elsewhere"}}},
     :zen/desc "Supporting evidence"}},
   :abatement
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:string :dateTime :Range :Period :Age}},
    :type zen/map,
    :keys
    {:dateTime {:confirms #{hl7-fhir-r4-core.dateTime/schema}},
     :_dateTime {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :Age {:confirms #{hl7-fhir-r4-core.Age/schema}},
     :Period {:confirms #{hl7-fhir-r4-core.Period/schema}},
     :Range {:confirms #{hl7-fhir-r4-core.Range/schema}},
     :string {:confirms #{hl7-fhir-r4-core.string/schema}},
     :_string {:confirms #{hl7-fhir-r4-core.Element/schema}}},
    :zen/desc "When in resolution/remission"},
   :asserter
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema
       hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema
       hl7-fhir-r4-core.RelatedPerson/schema}},
    :zen/desc "Person who asserts this condition"},
   :note
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Annotation/schema},
     :zen/desc "Additional information about the Condition"}},
   :recordedDate
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "Date record was first recorded"},
   :_recordedDate {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :recorder
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema
       hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema
       hl7-fhir-r4-core.RelatedPerson/schema}},
    :zen/desc "Who recorded the condition"},
   :severity
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.condition-severity/value-set,
     :strength :preferred},
    :zen/desc "Subjective severity of condition"},
   :code
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU},
    :zen/desc "Identification of the condition, problem or diagnosis"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :fhir/flags #{:SU},
     :zen/desc "External Ids for this condition"}},
   :bodySite
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
     :fhir/flags #{:SU},
     :zen/desc "Anatomical location, if relevant"}},
   :verificationStatus
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.condition-ver-status/value-set,
     :strength :required},
    :zen/desc
    "unconfirmed | provisional | differential | confirmed | refuted | entered-in-error"},
   :subject
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.Patient/schema hl7-fhir-r4-core.Group/schema}},
    :zen/desc "Who has the condition?"}},
  :zen.fhir/type "Condition"}}