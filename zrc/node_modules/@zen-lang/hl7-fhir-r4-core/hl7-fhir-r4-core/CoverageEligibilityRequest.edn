{ns hl7-fhir-r4-core.CoverageEligibilityRequest,
 import
 #{hl7-fhir-r4-core.Period
   hl7-fhir-r4-core.value-set.fm-status
   hl7-fhir-r4-core.BackboneElement
   hl7-fhir-r4-core.SimpleQuantity
   hl7-fhir-r4-core.Condition
   hl7-fhir-r4-core.code
   zen.fhir
   hl7-fhir-r4-core.Money
   hl7-fhir-r4-core.boolean
   hl7-fhir-r4-core.dateTime
   hl7-fhir-r4-core.string
   hl7-fhir-r4-core.Patient
   hl7-fhir-r4-core.date
   hl7-fhir-r4-core.PractitionerRole
   hl7-fhir-r4-core.Quantity
   hl7-fhir-r4-core.Coverage
   hl7-fhir-r4-core.Reference
   hl7-fhir-r4-core.positiveInt
   hl7-fhir-r4-core.Element
   hl7-fhir-r4-core.DomainResource
   hl7-fhir-r4-core.Practitioner
   hl7-fhir-r4-core.value-set.eligibilityrequest-purpose
   hl7-fhir-r4-core.CodeableConcept
   hl7-fhir-r4-core.Identifier
   hl7-fhir-r4-core.Location
   hl7-fhir-r4-core.Organization},
 schema
 {:zen.fhir/version "0.5.18",
  :confirms
  #{hl7-fhir-r4-core.DomainResource/schema zen.fhir/Resource},
  :zen/tags #{zen/schema zen.fhir/base-schema},
  :zen.fhir/profileUri
  "http://hl7.org/fhir/StructureDefinition/CoverageEligibilityRequest",
  :require #{:patient :purpose :created :insurer :status},
  :type zen/map,
  :zen/desc
  "The CoverageEligibilityRequest provides patient and insurance coverage information to an insurer for them to respond, in the form of an CoverageEligibilityResponse, with information regarding whether the stated coverage is valid and in-force and optionally to provide the insurance details of the policy.",
  :keys
  {:_created {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :patient
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Patient/schema}},
    :zen/desc "Intended recipient of products and services"},
   :insurance
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:focal
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :zen/desc "Applicable coverage"},
      :_focal {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :coverage
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers #{hl7-fhir-r4-core.Coverage/schema}},
       :zen/desc "Insurance information"},
      :businessArrangement
      {:confirms #{hl7-fhir-r4-core.string/schema},
       :zen/desc "Additional provider contract number"},
      :_businessArrangement
      {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:coverage},
     :zen/desc "Patient insurance information"}},
   :facility
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference {:refers #{hl7-fhir-r4-core.Location/schema}},
    :zen/desc "Servicing facility"},
   :enterer
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc "Author"},
   :supportingInfo
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:sequence
      {:confirms #{hl7-fhir-r4-core.positiveInt/schema},
       :zen/desc "Information instance identifier"},
      :_sequence {:confirms #{hl7-fhir-r4-core.Element/schema}},
      :information
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference {:refers #{}},
       :zen/desc "Data to be provided"},
      :appliesToAll
      {:confirms #{hl7-fhir-r4-core.boolean/schema},
       :zen/desc "Applies to all items"},
      :_appliesToAll {:confirms #{hl7-fhir-r4-core.Element/schema}}},
     :require #{:sequence :information},
     :zen/desc "Supporting information"}},
   :purpose
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.code/schema},
     :fhir/flags #{:SU},
     :zen.fhir/value-set
     {:symbol
      hl7-fhir-r4-core.value-set.eligibilityrequest-purpose/value-set,
      :strength :required},
     :zen/desc
     "auth-requirements | benefits | discovery | validation"},
    :minItems 1},
   :item
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
     :type zen/map,
     :keys
     {:category
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Benefit classification"},
      :facility
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.Organization/schema
          hl7-fhir-r4-core.Location/schema}},
       :zen/desc "Servicing facility"},
      :diagnosis
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.BackboneElement/schema},
        :type zen/map,
        :keys
        {:diagnosis
         {:fhir/polymorphic true,
          :exclusive-keys #{#{:CodeableConcept :Reference}},
          :type zen/map,
          :keys
          {:CodeableConcept
           {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema}},
           :Reference
           {:confirms
            #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
            :zen.fhir/reference
            {:refers #{hl7-fhir-r4-core.Condition/schema}}}},
          :zen/desc "Nature of illness or problem"}},
        :zen/desc "Applicable diagnosis"}},
      :modifier
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
        :zen/desc "Product or service billing modifiers"}},
      :_supportingInfoSequence
      {:type zen/vector,
       :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
      :productOrService
      {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
       :zen/desc "Billing, service, product, or drug code"},
      :quantity
      {:confirms
       #{hl7-fhir-r4-core.Quantity/schema
         hl7-fhir-r4-core.SimpleQuantity/schema},
       :zen/desc "Count of products or services"},
      :provider
      {:confirms
       #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
       :zen.fhir/reference
       {:refers
        #{hl7-fhir-r4-core.PractitionerRole/schema
          hl7-fhir-r4-core.Practitioner/schema}},
       :zen/desc "Perfoming practitioner"},
      :supportingInfoSequence
      {:type zen/vector,
       :every
       {:confirms #{hl7-fhir-r4-core.positiveInt/schema},
        :zen/desc "Applicable exception or supporting information"}},
      :unitPrice
      {:confirms #{hl7-fhir-r4-core.Money/schema},
       :zen/desc "Fee, charge or cost per item"},
      :detail
      {:type zen/vector,
       :every
       {:confirms
        #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
        :zen.fhir/reference {:refers #{}},
        :zen/desc "Product or service details"}}},
     :zen/desc "Item to be evaluated for eligibiity"}},
   :_status {:confirms #{hl7-fhir-r4-core.Element/schema}},
   :created
   {:confirms #{hl7-fhir-r4-core.dateTime/schema},
    :fhir/flags #{:SU},
    :zen/desc "Creation date"},
   :serviced
   {:fhir/polymorphic true,
    :exclusive-keys #{#{:date :Period}},
    :type zen/map,
    :keys
    {:date {:confirms #{hl7-fhir-r4-core.date/schema}},
     :_date {:confirms #{hl7-fhir-r4-core.Element/schema}},
     :Period {:confirms #{hl7-fhir-r4-core.Period/schema}}},
    :zen/desc "Estimated date or dates of service"},
   :_purpose
   {:type zen/vector,
    :every {:confirms #{hl7-fhir-r4-core.Element/schema}}},
   :insurer
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :fhir/flags #{:SU},
    :zen.fhir/reference
    {:refers #{hl7-fhir-r4-core.Organization/schema}},
    :zen/desc "Coverage issuer"},
   :priority
   {:confirms #{hl7-fhir-r4-core.CodeableConcept/schema},
    :zen/desc "Desired processing priority"},
   :status
   {:confirms #{hl7-fhir-r4-core.code/schema},
    :fhir/flags #{:SU :?!},
    :zen.fhir/value-set
    {:symbol hl7-fhir-r4-core.value-set.fm-status/value-set,
     :strength :required},
    :zen/desc "active | cancelled | draft | entered-in-error"},
   :identifier
   {:type zen/vector,
    :every
    {:confirms #{hl7-fhir-r4-core.Identifier/schema},
     :zen/desc "Business Identifier for coverage eligiblity request"}},
   :provider
   {:confirms #{hl7-fhir-r4-core.Reference/schema zen.fhir/Reference},
    :zen.fhir/reference
    {:refers
     #{hl7-fhir-r4-core.PractitionerRole/schema
       hl7-fhir-r4-core.Organization/schema
       hl7-fhir-r4-core.Practitioner/schema}},
    :zen/desc "Party responsible for the request"}},
  :zen.fhir/type "CoverageEligibilityRequest"}}